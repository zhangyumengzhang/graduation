d () {\r\n    // 基于准备好的dom，初始化echarts实例\r\n    this.data = []\r\n    this.data2 = []\r\n    var myChart = echarts.init(document.getElementById('main'))\r\n    const { data: res } = await this.axios.post('/storeOrderSum', {\r\n      store_id: window.sessionStorage.getItem('storeid')\r\n    })\r\n    if (res.status !== '1') {\r\n      return this.$message.error('获取数据失败')\r\n    }\r\n    console.log(res.data)\r\n    for (const item1 of res.data) {\r\n      this.data.push(item1.日期)\r\n      this.data2.push(item1.销量)\r\n    }\r\n    // 指定图表的配置项和数据\r\n    var option = {\r\n      title: {\r\n        text: '该店铺一周内每日销量'\r\n      },\r\n      tooltip: {},\r\n      legend: {\r\n        data: ['销量']\r\n      },\r\n      xAxis: {\r\n        data: this.data\r\n      },\r\n      yAxis: {},\r\n      series: [{\r\n        name: '销量',\r\n        type: 'bar',\r\n        data: this.data2\r\n      }]\r\n    }\r\n    // 使用刚指定的配置项和数据显示图表。\r\n    myChart.setOption(option)\r\n  },\r\n  methods () {}\r\n}\r\n</script>\r\n"]}]}
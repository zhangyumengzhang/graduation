{"remainingRequest":"D:\\2020summer\\vue_shop\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!D:\\2020summer\\vue_shop\\src\\views\\books\\books.vue?vue&type=script&lang=js&","dependencies":[{"path":"D:\\2020summer\\vue_shop\\src\\views\\books\\books.vue","mtime":1594276033120},{"path":"D:\\2020summer\\vue_shop\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1594033213228},{"path":"D:\\2020summer\\vue_shop\\node_modules\\babel-loader\\lib\\index.js","mtime":1594033213222},{"path":"D:\\2020summer\\vue_shop\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1594033213228},{"path":"D:\\2020summer\\vue_shop\\node_modules\\vue-loader\\lib\\index.js","mtime":1594033220124}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["books.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsMA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"books.vue","sourceRoot":"src/views/books","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑导航区域 -->\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>图书管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>图书列表</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n\r\n    <!-- 卡片视图区域 -->\r\n    <el-card class=\"box-card\">\r\n      <!-- 搜索与添加区域 -->\r\n       <el-row gutter=\"20\">\r\n         <el-col :span=\"8\">\r\n           <el-input placeholder=\"请输入内容\" v-model=\"queryInfo.query\" clearable\r\n           @clear=\"getUserList\">\r\n             <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"getUserList\"></el-button>\r\n          </el-input>\r\n          </el-col>\r\n          <el-col :span=\"4\">\r\n            <el-button type=\"primary\" @click=\"addDialogVisible = true\">添加图书</el-button>\r\n          </el-col>\r\n       </el-row>\r\n\r\n       <!-- 图书列表区 -->\r\n       <el-table :data=\"booklist\" border stripe>\r\n         <el-table-column type=\"index\"></el-table-column>\r\n         <el-table-column label=\"图书名称\" prop=\"bookname\"></el-table-column>\r\n         <el-table-column label=\"出版社\" prop=\"press\"></el-table-column>\r\n         <el-table-column label=\"作者\" prop=\"author\"></el-table-column>\r\n         <el-table-column label=\"价格\" prop=\"price\"></el-table-column>\r\n          <el-table-column label=\"状态(上/下架)\" prop=\"state\">\r\n            <template slot-scope=\"scope\">\r\n              <el-switch v-model=\"scope.row.mg_state\" @change=\"bookStateChanged(scope.row)\">\r\n              </el-switch>\r\n            </template>\r\n          </el-table-column>\r\n         <el-table-column label=\"操作\" width=\"180px\">\r\n            <template slot-scope=\"scope\">\r\n             <!-- 修改按钮 -->\r\n             <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\" @click=\"showEditDialog(scope.row.id)\"></el-button>\r\n             <!-- 删除按钮 -->\r\n             <el-button type=\"danger\" icon=\"el-icon-delete\" size=\"mini\" @click=\"removeBookbyId(scope.row.id)\"></el-button>\r\n             <!-- 查看详情 -->\r\n             <el-tooltip class=\"item\" effect=\"dark\" content=\"查看详情\" placement=\"top\" :enterable=\"false\">\r\n               <el-button type=\"warning\" icon=\"el-icon-more\" size=\"mini\" @click=\"showinfoDialog(scope.row.id)\"></el-button>\r\n             </el-tooltip>\r\n           </template>\r\n         </el-table-column>\r\n       </el-table>\r\n\r\n       <!-- 分页区域 -->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryInfo.pagenum\"\r\n        :page-sizes=\"[2, 5, 10]\"\r\n        :page-size=\"queryInfo.pagesize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\">\r\n      </el-pagination>\r\n    </el-card>\r\n\r\n     <!-- 添加对话框 -->\r\n    <el-dialog\r\n      title=\"添加图书\"\r\n      :visible.sync=\"addDialogVisible\"\r\n      width=\"50%\" @close=\"addDialogClosed\">\r\n\r\n      <!-- 内容主体区域-->\r\n      <el-form :model=\"addForm\" :rules=\"addFormRules\" ref=\"addFormRef\" label-width=\"80px\">\r\n        <el-form-item label=\"图书名称\" prop=\"bookname\">\r\n          <el-input autosize v-model=\"addForm.bookname\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"图书简介\" prop=\"bookoutline\">\r\n          <el-input autosize v-model=\"addForm.bookoutline\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"出版社\" prop=\"press\">\r\n          <el-input autosize v-model=\"addForm.press\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"出版时间\" prop=\"publishdate\">\r\n          <el-date-picker v-model=\"addForm.publishdate\" type=\"month\" placeholder=\"选择月\">\r\n          </el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"图书详情\" prop=\"bookdetail\">\r\n          <el-input autosize v-model=\"addForm.bookdetail\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"作者\" prop=\"author\">\r\n          <el-input autosize v-model=\"addForm.author\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"价格\" prop=\"price\">\r\n          <el-input autosize v-model=\"addForm.price\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"图书图片\" prop=\"bookpic\">\r\n          <el-upload class=\"upload-demo\"\r\n            action=\"https://jsonplaceholder.typicode.com/posts/\"\r\n            :on-preview=\"handlePreview\"\r\n            :on-remove=\"handleRemove\"\r\n            :file-list=\"fileList\"\r\n            list-type=\"picture\">\r\n            <el-button size=\"small\" type=\"primary\">点击上传</el-button>\r\n            <div slot=\"tip\" class=\"el-upload__tip\">只能上传jpg/png文件，且不超过500kb</div>\r\n          </el-upload>\r\n        </el-form-item>\r\n      </el-form>\r\n\r\n       <!-- 底部区域-->\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"addDialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"addBook\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n\r\n    <!-- 修改对话框 -->\r\n    <el-dialog\r\n      title=\"修改信息\"\r\n      :visible.sync=\"editdialogVisible\"\r\n      width=\"50%\" @close=\"editDialogClosed\">\r\n      <!-- 内容主体区域-->\r\n      <el-form :model=\"addForm\" :rules=\"addFormRules\" ref=\"addFormRef\" label-width=\"80px\">\r\n        <el-form-item label=\"图书名称\" prop=\"bookname\">\r\n          <el-input autosize v-model=\"addForm.bookname\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"图书简介\" prop=\"bookoutline\">\r\n          <el-input autosize v-model=\"addForm.bookoutline\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"出版社\" prop=\"press\">\r\n          <el-input autosize v-model=\"addForm.press\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"出版时间\" prop=\"publishdate\">\r\n          <el-date-picker v-model=\"addForm.publishdate\" type=\"month\" placeholder=\"选择月\">\r\n          </el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"图书详情\" prop=\"bookdetail\">\r\n          <el-input autosize v-model=\"addForm.bookdetail\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"作者\" prop=\"author\">\r\n          <el-input autosize v-model=\"addForm.author\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"价格\" prop=\"price\">\r\n          <el-input autosize v-model=\"addForm.price\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"图书图片\" prop=\"bookpic\">\r\n          <el-upload class=\"upload-demo\"\r\n            action=\"https://jsonplaceholder.typicode.com/posts/\"\r\n            :on-preview=\"handlePreview\"\r\n            :on-remove=\"handleRemove\"\r\n            :file-list=\"fileList\"\r\n            list-type=\"picture\">\r\n            <el-button size=\"small\" type=\"primary\">点击上传</el-button>\r\n            <div slot=\"tip\" class=\"el-upload__tip\">只能上传jpg/png文件，且不超过500kb</div>\r\n          </el-upload>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"editdialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"editBookInfo\">确 定</el-button>\r\n     </span>\r\n    </el-dialog>\r\n    <!-- 详情对话框 -->\r\n    <el-dialog\r\n      title=\"图书详情\"\r\n      :visible.sync=\"infodialogVisible\"\r\n      width=\"50%\" @close=\"infodialogClosed\">\r\n      <!-- 内容主体区域-->\r\n      <el-form :model=\"infoForm\" ref=\"infoFormRef\" label-width=\"80px\">\r\n        <el-form-item label=\"图书名称\" prop=\"bookname\">\r\n          <el-input autosize v-model=\"infoForm.bookname\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"图书简介\" prop=\"bookoutline\">\r\n          <el-input autosize v-model=\"infoForm.bookoutline\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"出版社\" prop=\"press\">\r\n          <el-input autosize v-model=\"infoForm.press\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"出版时间\" prop=\"publishdate\">\r\n          <el-input autosize v-model=\"infoForm.publishdate\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"图书详情\" prop=\"bookdetail\">\r\n          <el-input autosize v-model=\"infoForm.bookdetail\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"作者\" prop=\"author\">\r\n          <el-input autosize v-model=\"infoForm.author\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"价格\" prop=\"price\">\r\n          <el-input autosize v-model=\"infoForm.price\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"图书图片\" prop=\"bookpic\" disabled>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"infodialogVisible = false\">确 定</el-button>\r\n     </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 获取用户列表的参数对象\r\n      queryInfo: {\r\n        query: '',\r\n        // 当前页数\r\n        pagenum: '1',\r\n        // 当前每页显示多少条数据\r\n        pagesize: '2'\r\n      },\r\n      booklist: [],\r\n      total: 0,\r\n\r\n      // 控制添加对话框的显示和隐藏\r\n      addDialogVisible: false,\r\n      editdialogVisible: false,\r\n      infodialogVisible: false,\r\n      // 查询到的用户信息对象\r\n      editForm: {},\r\n      // 添加图书的表单数据\r\n      addForm: {\r\n        bookname: '',\r\n        bookoutline: '',\r\n        press: '',\r\n        publishdate: '',\r\n        bookdetail: '',\r\n        bookpic: '',\r\n        author: '',\r\n        price: '',\r\n        state: ''\r\n      },\r\n      infoForm: {\r\n        bookname: '',\r\n        bookoutline: '',\r\n        press: '',\r\n        publishdate: '',\r\n        bookdetail: '',\r\n        bookpic: '',\r\n        author: '',\r\n        price: '',\r\n        state: ''\r\n      },\r\n      // 添加表单的验证规则对象\r\n      addFormRules: {\r\n        bookname: [\r\n          { required: true, message: '请输入图书名称', trigger: 'blur' }\r\n        ],\r\n        bookoutline: [\r\n          { required: true, message: '请输入图书简介', trigger: 'blur' }\r\n        ],\r\n        press: [\r\n          { required: true, message: '请输入出版社', trigger: 'blur' }\r\n        ],\r\n        publishdate: [\r\n          { required: true, message: '请输入出版时间', trigger: 'blur' }\r\n        ],\r\n        bookdetail: [\r\n          { required: true, message: '请输入图书详情', trigger: 'blur' }\r\n        ],\r\n        author: [\r\n          { required: true, message: '请输入图书作者', trigger: 'blur' }\r\n        ],\r\n        price: [\r\n          { required: true, message: '请输入图书价格', trigger: 'blur' }\r\n        ],\r\n        bookpic: [\r\n          { required: true, message: '请上传图书图片', trigger: 'blur' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created () {\r\n    this.getBookList()\r\n  },\r\n  methods: {\r\n    async getBookList () {\r\n      // 获取的请求\r\n      const { data: res } = await this.$http.get('users', { params: this.queryInfo })\r\n      console.log(res)\r\n      if (res.meta.status !== 200) {\r\n        return this.$message.error('获取用户列表失败')\r\n      }\r\n      // this.$message.success('获取用户列表成功')\r\n      this.booklist = res.data.users\r\n      this.total = res.data.total\r\n      console.log(res)\r\n    },\r\n    // 监听pagesize改变\r\n    handleSizeChange (newSize) {\r\n      console.log(newSize)\r\n      this.queryInfo.pagesize = newSize\r\n      this.getBookList()\r\n    },\r\n    // 监听页码值改变的事件\r\n    handleCurrentChange (newPage) {\r\n      console.log(newPage)\r\n      this.queryInfo.pagenum = newPage\r\n      this.getBookList()\r\n    },\r\n    // 监听添加对话框的关闭\r\n    addDialogClosed () {\r\n      this.$refs.addFormRef.resetFields()\r\n    },\r\n\r\n    // 添加\r\n    addBook () {\r\n      this.$refs.addFormRef.validate(async valid => {\r\n        console.log(valid)\r\n        if (!valid) return\r\n\r\n        // 添加用户网络请求\r\n        const { data: res } = await this.$http.post('users', this.addForm)\r\n        if (res.meta.status !== 201) {\r\n          this.$message.error('添加失败')\r\n        }\r\n        this.$message.success('添加成功')\r\n        // 隐藏添加图书对话框\r\n        this.addDialogVisible = false\r\n        // 刷新图书列表\r\n        this.getBookList()\r\n      })\r\n    },\r\n\r\n    // 展示编辑图书的对话框\r\n    async showEditDialog (id) {\r\n      this.editdialogVisible = true\r\n      console.log(id)\r\n      const { data: res } = await this.$http.get('users/' + id)\r\n      if (res.mate.status !== 200) {\r\n        return this.$message.error('查询用户信息失败')\r\n      }\r\n      console.log(res.meta.status)\r\n      this.editForm = res.data\r\n    },\r\n    // 展示图书详情的对话框\r\n    async showinfoDialog (id) {\r\n      this.infodialogVisible = true\r\n      console.log(id)\r\n      const { data: res } = await this.$http.get('users/' + id)\r\n      if (res.mate.status !== 200) {\r\n        return this.$message.error('查询用户信息失败')\r\n      }\r\n      console.log(res.meta.status)\r\n      this.editForm = res.data\r\n    },\r\n    // 监听修改表单的关闭事件\r\n    editDialogClosed () {\r\n      this.$refs.editFormRef.resetFields()\r\n    },\r\n    // 修改图书信息并提交\r\n    editBookInfo () {\r\n      this.$refs.editFormRef.validate(async valid => {\r\n        if (!valid) return\r\n        // 发起修改请求\r\n        const { data: res } = await this.$http.put('users/' + this.editForm.id, this.editForm.email, this.editForm.mobile)\r\n        if (res.meta.status !== 200) {\r\n          this.$message.error('修改失败')\r\n        }\r\n\r\n        // 关闭对话框\r\n        this.editdialogVisible = false\r\n        // 刷新数据列表\r\n        this.getUserList()\r\n        // 提示修改成功\r\n        this.$message.success('修改成功')\r\n      })\r\n    },\r\n    // 根据ID删除\r\n    async removeBookbyId (id) {\r\n      // 弹框询问用户\r\n      const confirmresult = await this.$confirm('此操作将永久删除该图书, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).catch(err => err)\r\n      if (confirmresult !== 'confirm') {\r\n        return this.$message.info('已取消删除')\r\n      }\r\n      const { data: res } = await this.$delete('users/' + id)\r\n      if (res.mate.status !== 200) {\r\n        return this.$message.error('删除失败')\r\n      }\r\n      this.$message.success('删除成功')\r\n      this.getBookList()\r\n    },\r\n    // 监听状态开关的改变\r\n    async bookStateChanged (userinfo) {\r\n      this.$message.success('更改状态操作')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\r\n</style>\r\n"]}]}
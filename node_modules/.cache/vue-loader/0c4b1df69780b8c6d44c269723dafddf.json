type=\"text\" @click=\"changeType\">修改</el-button>\r\n      </div>\r\n    <!-- 内容主体区域-->\r\n      <el-form :model=\"shopForm\" :rules=\"shopFormRules\" ref=\"shopFormRef\" label-width=\"80px\">\r\n        <el-form-item label=\"店铺名称\" prop=\"store_name\">\r\n          <el-input v-model=\"shopForm.store_name\" :disabled=\"isdisabled\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"店铺地址\" prop=\"store_position\">\r\n          <el-input autosize v-model=\"shopForm.store_position\" :disabled=\"isdisabled\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"移动电话\" prop=\"store_phone_num\">\r\n          <el-input v-model=\"shopForm.store_phone_num\" :disabled=\"isdisabled\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"固定电话\" prop=\"store_telephone\">\r\n          <el-input v-model=\"shopForm.store_telephone\" :disabled=\"isdisabled\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"创建时间\" prop=\"created\">\r\n          <el-input v-model=\"shopForm.created\" disabled></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span class=\"bottombtn\" :hidden=\"isdisabled\">\r\n        <el-button  type=\"info\"  @click=\"changeType()\">取 消</el-button>\r\n        <el-button  type=\"primary\" @click=\"editShop()\">保 存</el-button>\r\n      </span>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 获取用户列表的参数对象\r\n      isdisabled: true,\r\n      // 添加用户的表单数据\r\n      shopForm: {\r\n        store_name: '',\r\n        store_position: '',\r\n        store_phone_num: '',\r\n        store_telephone: '',\r\n        created: ''\r\n      },\r\n      created () {\r\n        this.getshopInfo()\r\n      },\r\n      // 添加表单的验证规则对象\r\n      shopFormRules: {\r\n        store_name: [\r\n          { required: true, message: '请输入店铺名', trigger: 'blur' }\r\n        ],\r\n        store_position: [\r\n          { required: true, message: '请输入店铺地址', trigger: 'blur' }\r\n        ],\r\n        store_phone_num: [\r\n          { required: true, message: '请输入店铺移动电话', trigger: 'blur' }\r\n        ],\r\n        store_telephone: [\r\n          { required: true, message: '请输入店铺联系方式', trigger: 'blur' }\r\n        ],\r\n        created: [\r\n          { required: true, message: '请输入店铺成立时间', trigger: 'blur' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created () {\r\n    this.getshopInfo()\r\n  },\r\n  methods: {\r\n    async getshopInfo () {\r\n      // 获取店铺信息请求\r\n      const { data: res } = await this.axios.post('/selectStoreInfo', { email: window.sessionStorage.getItem('email') })\r\n      console.log(res)\r\n      if (res.status !== '1') {\r\n        return this.$message.error('获取店铺信息失败')\r\n      }\r\n      // this.$message.success('获取店铺信息成功')\r\n      this.shopForm = res.data[0]\r\n      window.sessionStorage.setItem('storeid', res.data[0].store_id)\r\n      console.log(res)\r\n    },\r\n    changeType () {\r\n      this.isdisabled = !this.isdisabled\r\n      this.getshopInfo()\r\n    },\r\n    async editShop () {\r\n      const { data: res } = await this.axios.post('/modifyStoreInfo', {\r\n        store_id: window.sessionStorage.getItem('storeid'),\r\n        store_name: this.shopForm.store_name,\r\n        store_position: this.shopForm.store_position,\r\n        store_phone_num: this.shopForm.store_phone_num,\r\n        store_telephone: this.shopForm.store_telephone\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') {\r\n        this.$message.error('修改店铺信息失败')\r\n        this.isdisabled = !this.disabled\r\n        this.getshopInfo()\r\n      }\r\n      // this.$message.success('修改店铺信息成功')\r\n      this.isdisabled = !this.disabled\r\n      this.getshopInfo()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.bottombtn {\r\n  float: right;\r\n  margin-bottom: 10px;\r\n}\r\n</style>\r\n"]}]}
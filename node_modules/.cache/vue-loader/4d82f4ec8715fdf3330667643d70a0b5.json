{"remainingRequest":"D:\\2020summer\\vue_shop\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!D:\\2020summer\\vue_shop\\src\\views\\books\\bookcategory.vue?vue&type=script&lang=js&","dependencies":[{"path":"D:\\2020summer\\vue_shop\\src\\views\\books\\bookcategory.vue","mtime":1594605287541},{"path":"D:\\2020summer\\vue_shop\\node_modules\\babel-loader\\lib\\index.js","mtime":1594033213222},{"path":"D:\\2020summer\\vue_shop\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1594033213228},{"path":"D:\\2020summer\\vue_shop\\node_modules\\vue-loader\\lib\\index.js","mtime":1594033220124}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["bookcategory.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsDA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"bookcategory.vue","sourceRoot":"src/views/books","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑导航区域 -->\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>图书管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>图书分类</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <!-- 卡片视图区域 -->\r\n    <el-card class=\"box-card\">\r\n       <!-- 类别列表区 -->\r\n       <el-table :data=\"catelist\" border stripe>\r\n         <el-table-column label=\"类别\" prop=\"categoryid\"></el-table-column>\r\n         <el-table-column label=\"类别介绍\" prop=\"cname\"></el-table-column>\r\n         <el-table-column label=\"操作\" width=\"140px\">\r\n            <template slot-scope=\"scope\">\r\n             <!-- 查看该类别下的图书按钮 -->\r\n             <el-tooltip class=\"item\" effect=\"dark\" content=\"查看该类别下的图书\" placement=\"top\" :enterable=\"false\">\r\n             <el-button type=\"primary\" icon=\"el-icon-folder-opened\" size=\"mini\" @click=\"getBookList(scope.row.categoryid)\"></el-button>\r\n             </el-tooltip>\r\n           </template>\r\n         </el-table-column>\r\n       </el-table>\r\n    </el-card>\r\n\r\n    <!-- 类别图书对话框 -->\r\n    <el-dialog\r\n      title=\"该类别下的图书\"\r\n      :visible.sync=\"infodialogVisible\"\r\n      width=\"65%\" @close=\"infoDialogClosed\">\r\n      <!-- 内容主体区域-->\r\n      <el-card class=\"box-card\">\r\n       <!-- 订单列表区 -->\r\n       <el-table :data=\"booklist\" border stripe>\r\n         <el-table-column label=\"图书ID\" prop=\"bookid\"></el-table-column>\r\n         <el-table-column label=\"图书名称\" prop=\"bookname\"></el-table-column>\r\n       </el-table>\r\n\r\n       <!-- 分页区域 -->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryInfo.pagenum\"\r\n        :page-sizes=\"[1,2, 5, 10]\"\r\n        :page-size=\"queryInfo.pagesize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\">\r\n      </el-pagination>\r\n    </el-card>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      catelist: [],\r\n      booklist: [],\r\n      queryInfo: {\r\n        store_id: window.sessionStorage.getItem('storeid'),\r\n        // 当前页数\r\n        pagenum: 1,\r\n        // 当前每页显示多少条数据\r\n        pagesize: 5\r\n      },\r\n      infoForm: {\r\n        bookid: '',\r\n        bookname: ''\r\n      },\r\n      total: 0,\r\n      infodialogVisible: false\r\n    }\r\n  },\r\n  created () {\r\n    this.getcateList()\r\n  },\r\n  methods: {\r\n    async getcateList () {\r\n      const { data: res } = await this.axios.post('/selectBookCate')\r\n      if (res.status !== '1') {\r\n        return this.$message.error('获取类别列表失败')\r\n      }\r\n      this.$message.success('获取类别列表成功')\r\n      this.catelist = res.data\r\n    },\r\n\r\n    async getBookList (id) {\r\n      this.infodialogVisible = true\r\n      window.sessionStorage.setItem('cateid', id)\r\n      const { data: res } = await this.axios.post('/selectBookByCate', {\r\n        category_id: id,\r\n        store_id: window.sessionStorage.getItem('storeid'),\r\n        // store_id: 1,\r\n        pagenum: this.queryInfo.pagenum,\r\n        pagesize: this.queryInfo.pagesize\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') {\r\n        return this.$message.error('获取该类别图书列表失败')\r\n      }\r\n      this.$message.success('获取该类别图书列表成功')\r\n      this.booklist = res.data\r\n      this.total = parseInt(res.sum)\r\n      console.log(res)\r\n    },\r\n    async getBookListin () {\r\n      this.infodialogVisible = true\r\n      const { data: res } = await this.axios.post('/selectBookByCate', {\r\n        category_id: window.sessionStorage.getItem('cateid'),\r\n        // store_id: window.sessionStorage.getItem('storeid'),\r\n        store_id: 1,\r\n        pagenum: this.queryInfo.pagenum,\r\n        pagesize: this.queryInfo.pagesize\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') {\r\n        return this.$message.error('获取该类别图书列表失败')\r\n      }\r\n      this.$message.success('获取该类别图书列表成功')\r\n      this.booklist = res.data\r\n      this.total = parseInt(res.sum)\r\n    },\r\n    // 监听pagesize改变\r\n    handleSizeChange (newSize) {\r\n      console.log(newSize)\r\n      this.queryInfo.pagesize = newSize\r\n      this.getBookListin()\r\n    },\r\n    // 监听页码值改变的事件\r\n    handleCurrentChange (newPage) {\r\n      console.log(newPage)\r\n      this.queryInfo.pagenum = newPage\r\n      this.getBookListin()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\r\n</style>\r\n"]}]}
le\r\n           @clear=\"getOrderList\">\r\n             <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"getOrderList\"></el-button>\r\n          </el-input>\r\n          </el-col>\r\n       </el-row>\r\n\r\n       <!-- 订单列表区 -->\r\n       <el-table :data=\"orderlist\" border stripe>\r\n         <el-table-column label=\"订单ID\" prop=\"orderid\"></el-table-column>\r\n         <el-table-column label=\"用户名称\" prop=\"username\"></el-table-column>\r\n         <el-table-column label=\"图书名称\" prop=\"bookname\"></el-table-column>\r\n         <el-table-column label=\"价格\" prop=\"payment\"></el-table-column>\r\n         <el-table-column label=\"创建时间\" prop=\"create_time\"></el-table-column>\r\n         <el-table-column label=\"订单状态\" prop=\"status\">\r\n           <template slot-scope=\"scope\">\r\n             <span style=\"float:center\"> 当前状态：{{status[scope.row.status].label}}</span>\r\n           </template>\r\n         </el-table-column>\r\n         <el-table-column label=\"操作\" width=\"180px\">\r\n            <template slot-scope=\"scope\">\r\n             <!-- 确认按钮 -->\r\n             <el-button type=\"primary\" icon=\"el-icon-check\" size=\"mini\" @click=\"showEditDialog(scope.row.orderid)\"></el-button>\r\n             <!-- 驳回按钮 -->\r\n             <el-button type=\"danger\" icon=\"el-icon-close\" size=\"mini\" @click=\"removeOrderbyId(scope.row.orderid)\"></el-button>\r\n           </template>\r\n         </el-table-column>\r\n       </el-table>\r\n\r\n       <!-- 分页区域 -->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryInfo.pagenum\"\r\n        :page-sizes=\"[1,2,5, 10]\"\r\n        :page-size=\"queryInfo.pagesize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\">\r\n      </el-pagination>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 状态选项value\r\n      status: [{\r\n        value: '0',\r\n        label: '未付款'\r\n      }, {\r\n        value: '1',\r\n        label: '已付款'\r\n      }, {\r\n        value: '2',\r\n        label: '未发货'\r\n      }, {\r\n        value: '3',\r\n        label: '已发货'\r\n      }, {\r\n        value: '4',\r\n        label: '交易成功'\r\n      }, {\r\n        value: '5',\r\n        label: '交易关闭'\r\n      }],\r\n      value: '',\r\n      // 获取用订单列表的参数对象\r\n      queryInfo: {\r\n        store_id: window.sessionStorage.getItem('storeid'),\r\n        // 当前页数\r\n        pagenum: 1,\r\n        // 当前每页显示多少条数据\r\n        pagesize: 2\r\n      },\r\n      orderlist: [],\r\n      total: 0,\r\n      // 控制添加对话框的显示和隐藏\r\n      editdialogVisible: false,\r\n      infodialogVisible: false,\r\n      editstatusdialogVisible: false,\r\n\r\n      editstatusForm: {\r\n        newstatus: ''\r\n      }\r\n    }\r\n  },\r\n  created () {\r\n    this.getOrderList()\r\n  },\r\n  methods: {\r\n    async getOrderList () {\r\n      const { data: res } = await this.axios.post('/selectOrders', {\r\n        store_id: 1,\r\n        pagenum: this.queryInfo.pagenum,\r\n        pagesize: this.queryInfo.pagesize\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') {\r\n        return this.$message.error('获取订单列表失败')\r\n      }\r\n      this.$message.success('获取订单列表成功')\r\n      this.orderlist = res.data\r\n      console.log(status)\r\n      this.total = parseInt(res.sum)\r\n    },\r\n    // 监听pagesize改变\r\n    handleSizeChange (newSize) {\r\n      console.log(newSize)\r\n      this.queryInfo.pagesize = newSize\r\n      this.getOrderList()\r\n    },\r\n    // 监听页码值改变的事件\r\n    handleCurrentChange (newPage) {\r\n      console.log(newPage)\r\n      this.queryInfo.pagenum = newPage\r\n      this.getOrderList()\r\n    },\r\n\r\n    // 根据ID删除\r\n    async removeOrderbyId (id) {\r\n      // 弹框询问用户\r\n      const confirmresult = await this.$confirm('此操作将永久删除该订单, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).catch(err => err)\r\n      if (confirmresult !== 'confirm') {\r\n        return this.$message.info('已取消删除')\r\n      }\r\n      const { data: res } = await this.axios.post('/deleteOrder', {\r\n        order_id: id\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') {\r\n        return this.$message.error('删除失败')\r\n      }\r\n      this.$message.success('删除成功')\r\n      this.getOrderList()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\r\n</style>\r\n"]}]}
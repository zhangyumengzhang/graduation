=\"append\" icon=\"el-icon-search\" @click=\"selectmusicQuery\"></el-button>\r\n          </el-input>\r\n          </el-col>\r\n       </el-row>\r\n       <!-- 类别列表区 -->\r\n       <el-table :data=\"musiclist\" border stripe>\r\n         <el-table-column label=\"音乐名称\" prop=\"audiotitle\"></el-table-column>\r\n         <el-table-column label=\"音乐类型\" prop=\"type\"></el-table-column>play\r\n          <el-table-column label=\"播放\" width=\"140px\">\r\n            <template>\r\n             <!-- 播放 -->\r\n             <el-tooltip class=\"item\" effect=\"dark\" content=\"播放\" placement=\"top\" :enterable=\"false\">\r\n             <el-button type=\"primary\" icon=\"el-icon-video-play\" size=\"mini\" @click=\"handlePlay('audio')\"></el-button>\r\n             </el-tooltip>\r\n               <audio id=\"audio\" src= 'http://localhost:8900/static/demo_input_0.wav'></audio>\r\n           </template>\r\n         </el-table-column>\r\n         <el-table-column label=\"收藏\" width=\"140px\">\r\n            <template slot-scope=\"scope\">\r\n             <!-- 收藏 -->\r\n             <el-tooltip class=\"item\" effect=\"dark\" content=\"收藏\" placement=\"top\" :enterable=\"false\">\r\n             <el-button type=\"primary\" icon=\"el-icon-star-on\" size=\"mini\" @click=\"changestar(scope.row.audiotitle)\"></el-button>\r\n             </el-tooltip>\r\n           </template>\r\n         </el-table-column>\r\n         <el-table-column label=\"下载\" width=\"140px\">\r\n            <template slot-scope=\"scope\">\r\n             <!-- 下载 -->\r\n             <el-tooltip class=\"item\" effect=\"dark\" content=\"下载\" placement=\"top\" :enterable=\"false\">\r\n             <el-button type=\"primary\" icon=\"el-icon-download\" size=\"mini\" @click=\"download(scope.row.audiotitle)\"></el-button>\r\n             </el-tooltip>\r\n           </template>\r\n         </el-table-column>\r\n         <el-table-column label=\"删除\" width=\"140px\">\r\n            <template slot-scope=\"scope\">\r\n             <!-- 删除 -->\r\n             <el-tooltip class=\"item\" effect=\"dark\" content=\"删除该音频\" placement=\"top\" :enterable=\"false\">\r\n             <el-button type=\"primary\" icon=\"el-icon-delete\" size=\"mini\" @click=\"deleteaudio(scope.row.audiotitle)\"></el-button>\r\n             </el-tooltip>\r\n           </template>\r\n         </el-table-column>\r\n       </el-table>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      musiclist: [],\r\n      queryInfo: {\r\n        username: window.sessionStorage.getItem('username'),\r\n        // 当前页数\r\n        pagenum: 1,\r\n        // 当前每页显示多少条数据\r\n        pagesize: 5\r\n      },\r\n      total: 0\r\n    }\r\n  },\r\n  created () {\r\n    this.getmusicList()\r\n  },\r\n  methods: {\r\n    handlePlay (id) {\r\n      document.getElementById(id).play()\r\n    },\r\n    async getmusicList () {\r\n      const { data: res } = await this.axios.post('/getstar', {\r\n        username: window.sessionStorage.getItem('username')\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') return this.$message.error(res.message)\r\n\r\n      if (res.status === '1') {\r\n        this.$message.success(res.message)\r\n        this.musiclist = res.songlists\r\n      };\r\n    },\r\n\r\n    // 改变收藏状态\r\n    async changestar () {\r\n\r\n    },\r\n    // 模糊查询音频\r\n    async selectBookQuery () {\r\n      // 获取的请求\r\n      console.log(this.queryInfo.pagesize)\r\n      const { data: res } = await this.axios.post('/selectmusicQuery', {\r\n        username: this.queryInfo.username,\r\n        // store_id: 1,\r\n        pagenum: this.queryInfo.pagenum,\r\n        pagesize: this.queryInfo.pagesize,\r\n        query: this.queryInfo.query\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') return this.$message.error('模糊获取图书列表失败')\r\n      this.$message.success('模糊获取图书列表成功')\r\n      this.musiclist = res.songlists\r\n      this.total = parseInt(res.sum)\r\n    },\r\n\r\n    async deleteaudio (musicname) {\r\n      // 弹框询问用户\r\n      const confirmresult = await this.$confirm('此操作将永久删除该音频, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).catch(err => err)\r\n      if (confirmresult !== 'confirm') {\r\n        return this.$message.info('已取消删除')\r\n      }\r\n\r\n      console.log(musicname)\r\n      const { data: res } = await this.axios.post('/delete', {\r\n        username: window.sessionStorage.getItem('username'),\r\n        musicname: musicname\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') return this.$message.error(res.message)\r\n      if (res.status === '1') {\r\n        this.$message.success(res.message)\r\n        this.getmusicList()\r\n      }\r\n    },\r\n    // 监听pagesize改变\r\n    handleSizeChange (newSize) {\r\n      console.log(newSize)\r\n      this.queryInfo.pagesize = newSize\r\n      this.getmusicList()\r\n    },\r\n    // 监听页码值改变的事件\r\n    handleCurrentChange (newPage) {\r\n      console.log(newPage)\r\n      this.queryInfo.pagenum = newPage\r\n      this.getmusicList()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\r\n</style>\r\n"]}]}
          <el-input placeholder=\"请输入内容\" clearable\r\n           @clear=\"getUserList\">\r\n             <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"getUserList\"></el-button>\r\n          </el-input>\r\n          </el-col>\r\n       </el-row>\r\n\r\n       <!-- 类别列表区 -->\r\n       <el-table :data=\"booklist\" border stripe>\r\n         <el-table-column type=\"index\"></el-table-column>\r\n         <el-table-column label=\"图书ID\" prop=\"bookid\"></el-table-column>\r\n         <el-table-column label=\"图书名称\" prop=\"bookname\"></el-table-column>\r\n         <el-table-column label=\"操作\" width=\"140px\">\r\n            <template slot-scope=\"scope\">\r\n             <!-- 修改按钮 -->\r\n             <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\" @click=\"showEditDialog(scope.row.id)\"></el-button>\r\n             <!-- 删除按钮 -->\r\n             <el-button type=\"danger\" icon=\"el-icon-delete\" size=\"mini\" @click=\"removeBookbyId(scope.row.id)\"></el-button>\r\n           </template>\r\n         </el-table-column>\r\n       </el-table>\r\n\r\n       <!-- 分页区域 -->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryInfo.pagenum\"\r\n        :page-sizes=\"[1,2, 5, 10]\"\r\n        :page-size=\"queryInfo.pagesize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\">\r\n      </el-pagination>\r\n    </el-card>\r\n\r\n    <!-- 修改对话框 -->\r\n    <el-dialog\r\n      title=\"修改信息\"\r\n      :visible.sync=\"editdialogVisible\"\r\n      width=\"50%\" @close=\"edieDialogClosed\">\r\n      <!-- 内容主体区域-->\r\n      <el-form :model=\"addForm\" :rules=\"addFormRules\" ref=\"addFormRef\" label-width=\"80px\">\r\n        <el-form-item label=\"类别名称\" prop=\"bookname\">\r\n          <el-input autosize v-model=\"addForm.bookname\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"类别介绍\" prop=\"bookoutline\">\r\n          <el-input autosize v-model=\"addForm.bookoutline\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"editdialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"editBookInfo\">确 定</el-button>\r\n     </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 获取用户列表的参数对象\r\n      queryInfo: {\r\n        query: '',\r\n        // 当前页数\r\n        pagenum: '1',\r\n        // 当前每页显示多少条数据\r\n        pagesize: '2'\r\n      },\r\n      booklist: [],\r\n      total: 0,\r\n      // 控制添加对话框的显示和隐藏\r\n      addDialogVisible: false,\r\n      editdialogVisible: false,\r\n      cname: window.sessionStorage.getItem('catename'),\r\n      // 查询到的用户信息对象\r\n      editForm: {},\r\n      // 添加用户的表单数据\r\n      addForm: {\r\n        bookname: '',\r\n        bookoutline: ''\r\n      },\r\n      // 添加表单的验证规则对象\r\n      addFormRules: {\r\n        bookname: [\r\n          { required: true, message: '请输入类别名称', trigger: 'blur' }\r\n        ],\r\n        bookoutline: [\r\n          { required: true, message: '请输入类别介绍', trigger: 'blur' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created () {\r\n    this.getBookList()\r\n  },\r\n  methods: {\r\n    async getBookList () {\r\n      const { data: res } = await this.axios.post('/selectBookByCate', {\r\n        category_id: window.sessionStorage.getItem('categoryid'),\r\n        store_id: window.sessionStorage.getItem('storeid'),\r\n        pagenum: this.queryInfo.pagenum,\r\n        pagesize: this.queryInfo.pagesize\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') {\r\n        return this.$message.error('获取该类别图书列表失败')\r\n      }\r\n      this.$message.success('获取该类别图书列表成功')\r\n      this.booklist = res.data\r\n      this.total = parseInt(res.sum)\r\n      console.log(res)\r\n    },\r\n    // 监听pagesize改变\r\n    handleSizeChange (newSize) {\r\n      console.log(newSize)\r\n      this.queryInfo.pagesize = newSize\r\n      this.getBookList()\r\n    },\r\n    // 监听页码值改变的事件\r\n    handleCurrentChange (newPage) {\r\n      console.log(newPage)\r\n      this.queryInfo.pagenum = newPage\r\n      this.getBookList()\r\n    },\r\n    // 根据ID删除\r\n    async removeBookbyId (id) {\r\n      // 弹框询问用户\r\n      const confirmresult = await this.$confirm('此操作将永久删除该类别和该类别下的图书, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).catch(err => err)\r\n      if (confirmresult !== 'confirm') {\r\n        return this.$message.info('已取消删除')\r\n      }\r\n      const { data: res } = await this.$delete('users/' + id)\r\n      if (res.mate.status !== 200) {\r\n        return this.$message.error('删除失败')\r\n      }\r\n      this.$message.success('删除成功')\r\n      this.getUserList()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\r\n</style>\r\n"]}]}
输入图书名称', trigger: 'blur' }\r\n        ],\r\n        outline: [\r\n          { required: true, message: '请输入图书简介', trigger: 'blur' }\r\n        ],\r\n        press: [\r\n          { required: true, message: '请输入出版社', trigger: 'blur' }\r\n        ],\r\n        publish_date: [\r\n          { required: true, message: '请输入出版时间', trigger: 'blur' }\r\n        ],\r\n        detail: [\r\n          { required: true, message: '请输入图书详情', trigger: 'blur' }\r\n        ],\r\n        author: [\r\n          { required: true, message: '请输入图书作者', trigger: 'blur' }\r\n        ],\r\n        price: [\r\n          { required: true, message: '请输入图书价格', trigger: 'blur' }\r\n        ],\r\n        book_category_id: [\r\n          { required: true, message: '请选择图书类别', trigger: 'blur' }\r\n        ],\r\n        is_shelf: [\r\n          { required: true, message: '请选择图书上架信息', trigger: 'blur' }\r\n        ],\r\n        pages: [\r\n          { required: true, message: '请输入总页数', trigger: 'blur' }\r\n        ],\r\n        store_mount: [\r\n          { required: true, message: '请输入库存量', trigger: 'blur' }\r\n        ],\r\n        market_price: [\r\n          { required: true, message: '请输入定价', trigger: 'blur' }\r\n        ],\r\n        member_price: [\r\n          { required: true, message: '请输入会员价', trigger: 'blur' }\r\n        ],\r\n        discount: [\r\n          { required: true, message: '请输入折扣', trigger: 'blur' }\r\n        ]\r\n      },\r\n      fileList: [{ name: '', url: '' }]\r\n    }\r\n  },\r\n  created () {\r\n    this.getBookList()\r\n  },\r\n  methods: {\r\n    // 上传图片\r\n    async beforeUpload (image) {\r\n      const pic = new FormData()\r\n      pic.append('picture', image)\r\n      const { data: res } = await this.axios.post('/UpPicture', pic)\r\n      console.log(res)\r\n      if (res.status !== '1') return this.$message.error('上传失败')\r\n      this.$message.success('成功')\r\n      this.addForm.image_url = res.url\r\n      return false\r\n    },\r\n    // 获取图书列表\r\n    async getBookList () {\r\n      // 获取的请求\r\n      console.log(this.queryInfo.pagesize)\r\n      const { data: res } = await this.axios.post('/selectBook', {\r\n        store_id: this.queryInfo.store_id,\r\n        // store_id: 1,\r\n        pagenum: this.queryInfo.pagenum,\r\n        pagesize: this.queryInfo.pagesize\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') return this.$message.error('获取图书列表失败')\r\n      this.$message.success('获取图书列表成功')\r\n      this.booklist = res.data\r\n      this.total = parseInt(res.sum)\r\n      this.booklist.forEach((item, i) => {\r\n        if (res.data[i].is_shelf === 1) {\r\n          res.data[i].is_shelf = '上架'\r\n        } else {\r\n          res.data[i].is_shelf = '下架'\r\n        }\r\n      })\r\n    },\r\n    // 模糊查询图书\r\n    async selectBookQuery () {\r\n      // 获取的请求\r\n      console.log(this.queryInfo.pagesize)\r\n      const { data: res } = await this.axios.post('/selectBookQuery', {\r\n        store_id: this.queryInfo.store_id,\r\n        // store_id: 1,\r\n        pagenum: this.queryInfo.pagenum,\r\n        pagesize: this.queryInfo.pagesize,\r\n        query: this.queryInfo.query\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') return this.$message.error('模糊获取图书列表失败')\r\n      this.$message.success('模糊获取图书列表成功')\r\n      this.booklist = res.data\r\n      this.total = parseInt(res.sum)\r\n      this.booklist.forEach((item, i) => {\r\n        console.log(res.data[i].author)\r\n        console.log(res.data[i].is_shelf)\r\n        if (res.data[i].is_shelf === 1) {\r\n          res.data[i].is_shelf = '上架'\r\n        } else {\r\n          res.data[i].is_shelf = '下架'\r\n        }\r\n      })\r\n    },\r\n    // 监听pagesize改变\r\n    handleSizeChange (newSize) {\r\n      console.log(newSize)\r\n      this.queryInfo.pagesize = newSize\r\n      this.getBookList()\r\n    },\r\n    // 监听页码值改变的事件\r\n    handleCurrentChange (newPage) {\r\n      console.log(newPage)\r\n      this.queryInfo.pagenum = newPage\r\n      this.getBookList()\r\n    },\r\n    // 监听添加对话框的关闭\r\n    addDialogClosed () {\r\n      this.$refs.addFormRef.resetFields()\r\n    },\r\n    // 监听修改表单的关闭事件\r\n    infodialogClosed () {\r\n      this.changeType()\r\n      this.$refs.infoFormRef.resetFields()\r\n    },\r\n    // 添加图书请求\r\n    addBook () {\r\n      this.addForm.store_id = window.sessionStorage.getItem('storeid')\r\n      this.addForm.cname = this.options1[this.addForm.book_category_id - 1].label\r\n      this.$refs.addFormRef.validate(async valid => {\r\n        console.log(this.addForm)\r\n        if (!valid) return\r\n        // 添加图书网络请求\r\n        const { data: res } = await this.axios.post('/addBook', this.addForm)\r\n        console.log(this.axios.post)\r\n        console.log(res)\r\n        if (res.status !== '1') {\r\n          this.$message.error('添加失败')\r\n        }\r\n        this.$message.success('添加成功')\r\n        // 隐藏添加图书对话框\r\n        this.addDialogVisible = false\r\n        // 重置添加列表\r\n        this.$refs.addFormRef.resetFields()\r\n        // 刷新图书列表\r\n        this.getBookList()\r\n      })\r\n    },\r\n\r\n    // 展示图书详情的对话框\r\n    async showinfoDialog (id) {\r\n      this.infodialogVisible = true\r\n      console.log(id)\r\n      const { data: res } = await this.axios.post('/selectBookinfo', {\r\n        bookid: id\r\n      }\r\n      )\r\n      if (res.status !== '1') {\r\n        return this.$message.error('查询图书信息失败')\r\n      }\r\n      console.log(res)\r\n      this.infoForm.author = res.author\r\n      this.infoForm.bookid = res.bookid\r\n      this.infoForm.image_url = res.image_url\r\n      this.infoForm.is_shelf = res.is_shelf\r\n      this.infoForm.name = res.name\r\n      this.infoForm.outline = res.outline\r\n      this.infoForm.press = res.press\r\n      this.infoForm.price = res.price\r\n      this.infoForm.publish_date = res.publish_date\r\n      this.infoForm.detail = res.detail\r\n      this.infoForm.size = res.size\r\n      this.infoForm.version = res.version\r\n      this.infoForm.translator = res.translator\r\n      this.infoForm.pages = res.pages\r\n      this.infoForm.catalog = res.catalog\r\n      this.infoForm.market_price = res.market_price\r\n      this.infoForm.member_price = res.member_price\r\n      this.infoForm.discount = res.discount\r\n      this.infoForm.store_mount = res.store_mount\r\n      this.infoForm.pack_style = res.pack_style\r\n      this.infoForm.cname = res.cname\r\n      this.infoForm.book_category_id = res.book_category_id\r\n      this.infoForm.description = res.description\r\n      this.infoForm.cata = res.cata\r\n      this.infoForm.content = res.content\r\n      this.infoForm.isbn = res.isbn\r\n    },\r\n\r\n    // 修改图书信息并提交\r\n    editBookInfo () {\r\n      // this.changeType()\r\n      this.$refs.infoFormRef.validate(async valid => {\r\n        if (!valid) return\r\n        // 发起修改请求\r\n        const { data: res } = await this.axios.post('/modifyBook', {\r\n          book_id: this.infoForm.bookid,\r\n          book_category_id: this.infoForm.book_category_id,\r\n          store_id: window.sessionStorage.getItem('storeid'),\r\n          name: this.infoForm.name,\r\n          outline: this.infoForm.outline,\r\n          detail: this.infoForm.detail,\r\n          press: this.infoForm.press,\r\n          author: this.infoForm.author,\r\n          publish_date: this.infoForm.publish_date,\r\n          size: this.infoForm.size,\r\n          version: this.infoForm.version,\r\n          translator: this.infoForm.translator,\r\n          isbn: this.infoForm.isbn,\r\n          price: this.infoForm.price,\r\n          pages: this.infoForm.pages,\r\n          catalog: this.infoForm.catalog,\r\n          market_price: this.infoForm.market_price,\r\n          member_price: this.infoForm.member_price,\r\n          discount: this.infoForm.discount,\r\n          image_url: this.infoForm.image_url,\r\n          store_mount: this.infoForm.store_mount,\r\n          pack_style: this.infoForm.pack_style,\r\n          is_shelf: this.infoForm.is_shelf,\r\n          cname: this.infoForm.cname,\r\n          description: this.infoForm.description,\r\n          cata: this.infoForm.cata,\r\n          content: this.infoForm.content\r\n        })\r\n        console.log(res)\r\n        if (res.status !== '1') {\r\n          this.$message.error('修改失败')\r\n        }\r\n        // 提示修改成功\r\n        this.$message.success('修改成功')\r\n        // 关闭对话框\r\n        this.infodialogVisible = false\r\n        // 刷新数据列表\r\n        this.getBookList()\r\n      })\r\n    },\r\n\r\n    // 根据ID删除\r\n    async removeBookbyId (id) {\r\n      // 弹框询问用户\r\n      const confirmresult = await this.$confirm('此操作将永久删除该图书, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).catch(err => err)\r\n      if (confirmresult !== 'confirm') {\r\n        return this.$message.info('已取消删除')\r\n      }\r\n      const { data: res } = await this.axios.post('/deleteBook', {\r\n        bookid: id\r\n      })\r\n      if (res.status !== '1') {\r\n        return this.$message.error('删除失败')\r\n      }\r\n      this.$message.success('删除成功')\r\n      this.getBookList()\r\n    },\r\n\r\n    // 修改开启\r\n    changeType () {\r\n      this.isdisabled = !this.isdisabled\r\n    },\r\n    //\r\n    handleRemove (file, fileList) {\r\n      console.log(file, fileList)\r\n    },\r\n    //\r\n    handlePreview (file) {\r\n      console.log(file)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.editbtn{\r\n  float: right;\r\n  line-height: 15px;\r\n}\r\n</style>\r\n"]}]}
AA,CAAA,oBAAA,EAAA,IAAA,EAAA;AACA,kBAAA,iBAAA,EAAA,IADA;AAEA,kBAAA,gBAAA,EAAA,IAFA;AAGA,kBAAA,IAAA,EAAA;AAHA,iBAAA,EAIA,KAJA,CAIA,UAAA,GAAA;AAAA,yBAAA,GAAA;AAAA,iBAJA,CAFA;;AAAA;AAEA,gBAAA,aAFA;;AAAA,sBAOA,aAAA,KAAA,SAPA;AAAA;AAAA;AAAA;;AAAA,kDAQA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CARA;;AAAA;AAAA;AAAA,uBAUA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,cAAA,EAAA;AACA,kBAAA,QAAA,EAAA;AADA,iBAAA,CAVA;;AAAA;AAAA;AAUA,gBAAA,GAVA,yBAUA,IAVA;AAaA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AAbA,sBAcA,GAAA,CAAA,MAAA,KAAA,GAdA;AAAA;AAAA;AAAA;;AAAA,kDAeA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAfA;;AAAA;AAiBA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,YAAA;;AAlBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmBA;AApKA;AA7IA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑导航区域 -->\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>订单管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>订单列表</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n\r\n    <!-- 卡片视图区域 -->\r\n    <el-card class=\"box-card\">\r\n      <!-- 搜索与添加区域 -->\r\n       <el-row gutter=\"20\">\r\n         <el-col :span=\"8\">\r\n           <el-input placeholder=\"请输入内容\" v-model=\"queryInfo.query\" clearable\r\n           @clear=\"getOrderList\">\r\n             <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"getOrderList\"></el-button>\r\n          </el-input>\r\n          </el-col>\r\n       </el-row>\r\n\r\n       <!-- 订单列表区 -->\r\n       <el-table :data=\"orderlist\" border stripe>\r\n         <el-table-column label=\"订单ID\" prop=\"orderid\"></el-table-column>\r\n         <el-table-column label=\"用户名称\" prop=\"username\"></el-table-column>\r\n         <el-table-column label=\"图书名称\" prop=\"bookname\"></el-table-column>\r\n         <el-table-column label=\"价格\" prop=\"payment\"></el-table-column>\r\n         <el-table-column label=\"创建时间\" prop=\"create_time\"></el-table-column>\r\n         <el-table-column label=\"订单状态\" prop=\"status\">\r\n           <template slot-scope=\"scope\">\r\n             <span style=\"float:center\"> 当前状态：{{status[scope.row.status].label}}</span>\r\n             <div style=\"color: #409EFF;cursor:pointer;float:center\" @click=\"changestatus(scope.row)\">修改状态</div>\r\n           </template>\r\n         </el-table-column>\r\n         <el-table-column label=\"操作\" width=\"180px\">\r\n            <template slot-scope=\"scope\">\r\n             <!-- 修改按钮 -->\r\n             <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\" @click=\"showEditDialog(scope.row.orderid)\"></el-button>\r\n             <!-- 删除按钮 -->\r\n             <el-button type=\"danger\" icon=\"el-icon-delete\" size=\"mini\" @click=\"removeOrderbyId(scope.row.orderid)\"></el-button>\r\n             <!-- 导出订单 -->\r\n             <el-tooltip class=\"item\" effect=\"dark\" content=\"导出订单\" placement=\"top\" :enterable=\"false\">\r\n               <el-button type=\"warning\" icon=\"el-icon-more\" size=\"mini\" @click=\"showinfoDialog(scope.row.orderid)\"></el-button>\r\n             </el-tooltip>\r\n           </template>\r\n         </el-table-column>\r\n       </el-table>\r\n\r\n       <!-- 分页区域 -->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryInfo.pagenum\"\r\n        :page-sizes=\"[1,2,5, 10]\"\r\n        :page-size=\"queryInfo.pagesize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\">\r\n      </el-pagination>\r\n    </el-card>\r\n\r\n    <!-- 修改对话框 -->\r\n    <el-dialog\r\n      title=\"修改订单\"\r\n      :visible.sync=\"editdialogVisible\"\r\n      width=\"50%\" @close=\"editDialogClosed\">\r\n      <!-- 内容主体区域-->\r\n      <el-form :model=\"editForm\" :rules=\"editFormRules\" ref=\"editFormRef\" label-width=\"120px\">\r\n        <el-form-item label=\"订单ID\" prop=\"orderid\">\r\n          <el-input autosize v-model=\"editForm.orderid\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"收件人姓名\" prop=\"receiver_name\">\r\n          <el-input autosize v-model=\"editForm.receiver_name\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"收件人电话\" prop=\"receiver_mobile\">\r\n          <el-input autosize v-model=\"editForm.receiver_mobile\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"收货省市\" prop=\"receiver_state\">\r\n          <el-input autosize v-model=\"editForm.receiver_state\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"收货城市\" prop=\"receiver_city\" >\r\n          <el-input autosize v-model=\"editForm.receiver_city\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"收货地区\" prop=\"receiver_district\">\r\n          <el-input autosize v-model=\"editForm.receiver_district\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"收货地址\" prop=\"receiver_address\">\r\n          <el-input autosize v-model=\"editForm.receiver_address\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"editdialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"editBookInfo\">确 定</el-button>\r\n     </span>\r\n    </el-dialog>\r\n\r\n    <!-- 修改状态对话框 -->\r\n    <el-dialog\r\n      title=\"修改状态\"\r\n      :visible.sync=\"editstatusdialogVisible\"\r\n      width=\"50%\" @close=\"editstatusDialogClosed\">\r\n      <!-- 内容主体区域-->\r\n      <el-form :model=\"editstatusForm\" :rules=\"editstatusFormRules\" ref=\"editstatusFormRef\" label-width=\"120px\">\r\n        <el-form-item label=\"订单ID\" prop=\"newstatus\">\r\n           <el-select v-model=\"value\" clearable placeholder=\"请选择 \">\r\n            <el-option\r\n              v-for=\"item in options\"\r\n              :key=\"item.value\"\r\n              :label=\"item.label\"\r\n              :value=\"item.value\"\r\n              :disabled=\"item.disabled\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"editstatusDialogClosed\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"confirmchangeStatus\">确 定</el-button>\r\n     </span>\r\n    </el-dialog>\r\n\r\n    <!-- 导出对话框 -->\r\n    <el-dialog\r\n      title=\"导出订单\"\r\n      :visible.sync=\"infodialogVisible\"\r\n      width=\"50%\" @close=\"infoDialogClosed\">\r\n      <!-- 内容主体区域-->\r\n      <el-form :model=\"infoForm\" ref=\"infoFormRef\" label-width=\"100px\">\r\n        <el-form-item label=\"订单ID\" prop=\"orderid\">\r\n          <el-input autosize v-model=\"infoForm.orderid\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"收件人姓名\" prop=\"receiver_name\">\r\n          <el-input autosize v-model=\"infoForm.receiver_name\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"收件人电话\" prop=\"receiver_mobile\">\r\n          <el-input autosize v-model=\"infoForm.receiver_mobile\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"收货省市\" prop=\"receiver_state\">\r\n          <el-input autosize v-model=\"infoForm.receiver_state\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"收货城市\" prop=\"receiver_city\" >\r\n          <el-input autosize v-model=\"infoForm.receiver_city\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"收货地区\" prop=\"receiver_district\">\r\n          <el-input autosize v-model=\"infoForm.receiver_district\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"收货地址\" prop=\"receiver_address\">\r\n          <el-input autosize v-model=\"infoForm.receiver_address\" disabled></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"infodialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"exportorder(this.infoForm.orderid)\">导 出</el-button>\r\n     </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 状态选项value\r\n      status: [{\r\n        value: '0',\r\n        label: '未付款'\r\n      }, {\r\n        value: '1',\r\n        label: '已付款'\r\n      }, {\r\n        value: '2',\r\n        label: '未发货'\r\n      }, {\r\n        value: '3',\r\n        label: '已发货'\r\n      }, {\r\n        value: '4',\r\n        label: '交易成功'\r\n      }, {\r\n        value: '5',\r\n        label: '交易关闭'\r\n      }],\r\n      options: [{}],\r\n      // 状态选项value\r\n      options1: [{\r\n        value: '0',\r\n        label: '未付款'\r\n      }, {\r\n        value: '1',\r\n        label: '已付款'\r\n      }],\r\n      options2: [{\r\n        value: '1',\r\n        label: '已付款'\r\n      }, {\r\n        value: '2',\r\n        label: '未发货'\r\n        // disabled: 'isdisabled2'\r\n      }],\r\n      options3: [{\r\n        value: '2',\r\n        label: '未发货'\r\n        // disabled:'isdisabled2'\r\n      }, {\r\n        value: '3',\r\n        label: '已发货'\r\n        // disabled:'isdisabled3'\r\n      }],\r\n      options4: [{\r\n        value: '3',\r\n        label: '已发货'\r\n        // disabled:'isdisabled3'\r\n      }, {\r\n        value: '4',\r\n        label: '交易成功'\r\n        // disabled:'isdisabled4'\r\n      }],\r\n      options5: [{\r\n        value: '4',\r\n        label: '交易成功'\r\n        // disabled:'isdisabled4'\r\n      }, {\r\n        value: '5',\r\n        label: '交易关闭'\r\n        // disabled:'isdisabled5'\r\n      }],\r\n      options6: [{\r\n        value: '5',\r\n        label: '交易关闭'\r\n        // disabled:'isdisabled5'\r\n      }],\r\n      value: '',\r\n      // 获取用订单列表的参数对象\r\n      queryInfo: {\r\n        store_id: window.sessionStorage.getItem('storeid'),\r\n        // 当前页数\r\n        pagenum: 1,\r\n        // 当前每页显示多少条数据\r\n        pagesize: 2\r\n      },\r\n      orderlist: [],\r\n      total: 0,\r\n      // 控制添加对话框的显示和隐藏\r\n      editdialogVisible: false,\r\n      infodialogVisible: false,\r\n      editstatusdialogVisible: false,\r\n\r\n      editstatusForm: {\r\n        newstatus: ''\r\n      },\r\n      // 编辑订单的表单数据\r\n      editForm: {\r\n        orderid: '',\r\n        receiver_name: '',\r\n        receiver_mobile: '',\r\n        receiver_state: '',\r\n        receiver_city: '',\r\n        receiver_district: '',\r\n        receiver_address: ''\r\n      },\r\n      infoForm: {\r\n        orderid: '',\r\n        receiver_name: '',\r\n        receiver_mobile: '',\r\n        receiver_state: '',\r\n        receiver_city: '',\r\n        receiver_district: '',\r\n        receiver_address: ''\r\n      },\r\n      // 添加表单的验证规则对象\r\n      editFormRules: {\r\n        receiver_name: [\r\n          { required: true, message: '请输入收件人姓名', trigger: 'blur' }\r\n        ],\r\n        receiver_mobile: [\r\n          { required: true, message: '请输入收件人电话', trigger: 'blur' }\r\n        ],\r\n        receiver_state: [\r\n          { required: true, message: '请输入收件省市', trigger: 'blur' }\r\n        ],\r\n        receiver_city: [\r\n          { required: true, message: '请输入收件城市', trigger: 'blur' }\r\n        ],\r\n        receiver_district: [\r\n          { required: true, message: '请输入收件地区', trigger: 'blur' }\r\n        ],\r\n        receiver_address: [\r\n          { required: true, message: '请输入收件地址', trigger: 'blur' }\r\n        ]\r\n      },\r\n      editstatusFormRules: {\r\n        newstatus: [\r\n          { required: true, message: '请输入订单状态', trigger: 'blur' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created () {\r\n    this.getOrderList()\r\n  },\r\n  methods: {\r\n    async getOrderList () {\r\n      const { data: res } = await this.axios.post('/selectOrders', {\r\n        store_id: 1,\r\n        pagenum: this.queryInfo.pagenum,\r\n        pagesize: this.queryInfo.pagesize\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') {\r\n        return this.$message.error('获取订单列表失败')\r\n      }\r\n      this.$message.success('获取订单列表成功')\r\n      this.orderlist = res.data\r\n      console.log(status)\r\n      this.total = parseInt(res.sum)\r\n    },\r\n    // 修改状态\r\n    changestatus (order) {\r\n      window.sessionStorage.setItem('orderid', order.orderid)\r\n      console.log(order.status)\r\n      if (order.status === 0) {\r\n        this.options = this.options1\r\n      } else if (order.status === 1) {\r\n        this.options = this.options2\r\n      } else if (order.status === 2) {\r\n        this.options = this.options3\r\n      } else if (order.status === 3) {\r\n        this.options = this.options4\r\n      } else if (order.status === 4) {\r\n        this.options = this.options5\r\n      } else if (order.status === 5) {\r\n        this.options = this.options6\r\n      }\r\n      this.editstatusdialogVisible = true\r\n    },\r\n    async confirmchangeStatus () {\r\n      const { data: res } = await this.axios.post('/modifyOrderStatus', {\r\n        order_id: window.sessionStorage.getItem('orderid'),\r\n        status: this.value\r\n      })\r\n      console.log(window.sessionStorage.getItem('orderid'))\r\n      console.log(this.editstatusForm.newstatus)\r\n      console.log(res)\r\n      if (res.status !== '1') return this.$message.error('修改状态失败')\r\n      this.$message.success('修改状态成功')\r\n      this.editstatusdialogVisible = false\r\n      this.editstatusDialogClosed()\r\n      this.getOrderList()\r\n    },\r\n    // 监听pagesize改变\r\n    handleSizeChange (newSize) {\r\n      console.log(newSize)\r\n      this.queryInfo.pagesize = newSize\r\n      this.getOrderList()\r\n    },\r\n    // 监听页码值改变的事件\r\n    handleCurrentChange (newPage) {\r\n      console.log(newPage)\r\n      this.queryInfo.pagenum = newPage\r\n      this.getOrderList()\r\n    },\r\n    // 展示编辑订单的对话框\r\n    async showEditDialog (id) {\r\n      this.editdialogVisible = true\r\n      console.log(id)\r\n      const { data: res } = await this.axios.post('/selectOrderShipping', {\r\n        order_id: id\r\n      })\r\n      if (res.status !== '1') {\r\n        // return this.$message.error('查询订单信息失败')\r\n      }\r\n      this.editForm.orderid = res.data[0].order_id\r\n      this.editForm.receiver_name = res.data[0].receiver_name\r\n      this.editForm.receiver_mobile = res.data[0].receiver_mobile\r\n      this.editForm.receiver_state = res.data[0].receiver_state\r\n      this.editForm.receiver_city = res.data[0].receiver_city\r\n      this.editForm.receiver_district = res.data[0].receiver_district\r\n      this.editForm.receiver_address = res.data[0].receiver_address\r\n    },\r\n    // 展示导出订单的对话框\r\n    async showinfoDialog (id) {\r\n      this.infodialogVisible = true\r\n      console.log(id)\r\n      console.log(this.infodialogVisible)\r\n      const { data: res } = await this.axios.post('/selectOrderShipping', {\r\n        order_id: id\r\n      })\r\n      if (res.status !== '1') {\r\n        return this.$message.error('查询订单信息失败')\r\n      }\r\n      this.$message.success('查询订单信息成功')\r\n      console.log(res)\r\n      this.infoForm.orderid = res.data[0].order_id\r\n      this.infoForm.receiver_name = res.data[0].receiver_name\r\n      this.infoForm.receiver_mobile = res.data[0].receiver_mobile\r\n      this.infoForm.receiver_state = res.data[0].receiver_state\r\n      this.infoForm.receiver_city = res.data[0].receiver_city\r\n      this.infoForm.receiver_district = res.data[0].receiver_district\r\n      this.infoForm.receiver_address = res.data[0].receiver_address\r\n    },\r\n    // 导出订单\r\n    exportorder (id) {\r\n      this.infodialogVisible = false\r\n      this.$message.success('导出操作')\r\n    },\r\n    // 监听编辑订单的关闭事件\r\n    editDialogClosed () {\r\n      this.$refs.editFormRef.resetFields()\r\n    },\r\n    // 监听修改状态的关闭事件\r\n    editstatusDialogClosed () {\r\n      this.editstatusdialogVisible = false\r\n      this.$refs.editstatusFormRef.resetFields()\r\n    },\r\n    // 监听导出订单对话框关闭\r\n    infoDialogClosed () {\r\n      this.$refs.infoFormRef.resetFields()\r\n    },\r\n    // 修改订单信息并提交\r\n    editBookInfo () {\r\n      this.$refs.editFormRef.validate(async valid => {\r\n        if (!valid) return\r\n        // 发起修改请求\r\n        const { data: res } = await this.axios.post('/modifyOrderShipping', {\r\n          order_id: this.editForm.orderid,\r\n          receiver_name: this.editForm.receiver_name,\r\n          receiver_mobile: this.editForm.receiver_mobile,\r\n          receiver_state: this.editForm.receiver_state,\r\n          receiver_city: this.editForm.receiver_city,\r\n          receiver_district: this.editForm.receiver_district,\r\n          receiver_address: this.editForm.receiver_address\r\n        })\r\n        if (res.status !== '1') {\r\n          this.$message.error('修改失败')\r\n        }\r\n        console.log(res)\r\n        // 关闭对话框\r\n        this.editdialogVisible = false\r\n        // 刷新数据列表\r\n        this.getOrderList()\r\n        // 提示修改成功\r\n        this.$message.success('修改成功')\r\n      })\r\n    },\r\n    // 根据ID删除\r\n    async removeOrderbyId (id) {\r\n      // 弹框询问用户\r\n      const confirmresult = await this.$confirm('此操作将永久删除该订单, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).catch(err => err)\r\n      if (confirmresult !== 'confirm') {\r\n        return this.$message.info('已取消删除')\r\n      }\r\n      const { data: res } = await this.axios.post('/deleteOrder', {\r\n        order_id: id\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') {\r\n        return this.$message.error('删除失败')\r\n      }\r\n      this.$message.success('删除成功')\r\n      this.getOrderList()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/orders"}]}
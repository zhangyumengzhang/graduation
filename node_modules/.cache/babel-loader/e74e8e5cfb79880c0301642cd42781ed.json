,cAAA,CAAA,OAAA,CAAA,OAAA;AAAA,iBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,yBAEA,IAFA;AAGA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AAHA,sBAIA,GAAA,CAAA,MAAA,KAAA,GAJA;AAAA;AAAA;AAAA;;AAAA,iDAKA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,UAAA,CALA;;AAAA;AAOA;AACA,gBAAA,KAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,CAAA,CAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA,KAXA;AAYA,IAAA,UAZA,wBAYA;AACA,WAAA,UAAA,GAAA,CAAA,KAAA,UAAA;AACA,KAdA;AAeA,IAAA,UAfA,wBAeA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,mBAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,UAAA,CAAA,KADA;AAEA,kBAAA,QAAA,EAAA,MAAA,CAAA,UAAA,CAAA,QAFA;AAGA,kBAAA,QAAA,EAAA,MAAA,CAAA,UAAA,CAAA,UAHA;AAIA,kBAAA,MAAA,EAAA,MAAA,CAAA,UAAA,CAAA,cAJA;AAKA,kBAAA,GAAA,EAAA,MAAA,CAAA,UAAA,CAAA,eALA;AAMA,kBAAA,OAAA,EAAA,MAAA,CAAA,UAAA,CAAA,eANA;AAOA,kBAAA,QAAA,EAAA,MAAA,CAAA,UAAA,CAAA;AAPA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,yBACA,IADA;AAUA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,oBAAA,GAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,UAAA;;AACA,kBAAA,MAAA,CAAA,UAAA,GAAA,CAAA,MAAA,CAAA,QAAA;;AACA,kBAAA,MAAA,CAAA,aAAA;AACA;;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA;;AACA,gBAAA,MAAA,CAAA,UAAA,GAAA,CAAA,MAAA,CAAA,QAAA;;AACA,gBAAA,MAAA,CAAA,aAAA;;AAlBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmBA;AAlCA;AAnCA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑导航区域 -->\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>个人中心</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <!-- 卡片视图区域 -->\r\n    <el-card class=\"box-card\">\r\n      <div slot=\"header\" class=\"clearfix\">\r\n        <span>个人信息</span>\r\n        <el-button style=\"float: right; padding: 3px 0\" type=\"text\" @click=\"changeType\">修改</el-button>\r\n      </div>\r\n    <!-- 内容主体区域-->\r\n      <el-form :model=\"personForm\" :rules=\"personFormRules\" ref=\"personFormRef\" label-width=\"80px\">\r\n         <el-form-item label=\"用户名称\" prop=\"username\">\r\n          <el-input v-model=\"personForm.username\" :disabled=\"isdisabled\"></el-input>\r\n        </el-form-item>\r\n         <el-form-item label=\"昵称\" prop=\"nickname\">\r\n          <el-input v-model=\"personForm.nickname\" :disabled=\"isdisabled\"></el-input>\r\n        </el-form-item>\r\n         <el-form-item label=\"性别\" prop=\"gender\">\r\n          <el-input v-model=\"personForm.gender\" :disabled=\"isdisabled\"></el-input>\r\n        </el-form-item>\r\n         <el-form-item label=\"年龄\" prop=\"age\">\r\n          <el-input v-model=\"personForm.age\" :disabled=\"isdisabled\"></el-input>\r\n        </el-form-item>\r\n          <el-form-item label=\"邮箱\" prop=\"email\">\r\n          <el-input v-model=\"personForm.email\" disabled></el-input>\r\n        </el-form-item>\r\n          <el-form-item label=\"国家\" prop=\"country\">\r\n          <el-input v-model=\"personForm.country\" :disabled=\"isdisabled\"></el-input>\r\n        </el-form-item>\r\n          <el-form-item label=\"地区\" prop=\"location\">\r\n          <el-input v-model=\"personForm.location\" :disabled=\"isdisabled\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span class=\"bottombtn\" :hidden=\"isdisabled\">\r\n        <el-button  type=\"info\"  @click=\"changeType()\">取 消</el-button>\r\n        <el-button  type=\"primary\" @click=\"editperson()\">保 存</el-button>\r\n      </span>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 获取用户列表的参数对象\r\n      isdisabled: true,\r\n      // 添加用户的表单数据\r\n      personForm: {\r\n        username: '',\r\n        nickname: '',\r\n        gender: '',\r\n        age: '',\r\n        email: '',\r\n        country: '',\r\n        location: ''\r\n      },\r\n      // 添加表单的验证规则对象\r\n      personFormRules: {\r\n        username: [\r\n          { required: true, message: '请输入用户名', trigger: 'blur' }\r\n        ],\r\n        gender: [\r\n          { required: true, message: '请输入性别', trigger: 'blur' }\r\n        ],\r\n        age: [\r\n          { required: true, message: '请输入年龄', trigger: 'blur' }\r\n        ],\r\n        email: [\r\n          { required: true, message: '请输入邮箱', trigger: 'blur' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created () {\r\n    this.getpersonInfo()\r\n  },\r\n  methods: {\r\n    async getpersonInfo () {\r\n      // 获取用户信息请求\r\n      const { data: res } = await this.axios.post('/selectUserDetail', { email: window.sessionStorage.getItem('email') })\r\n      console.log(res)\r\n      if (res.status !== '1') {\r\n        return this.$message.error('获取用户信息失败')\r\n      }\r\n      // this.$message.success('获取用户信息成功')\r\n      this.personForm = res.data[0]\r\n      console.log(res)\r\n    },\r\n    changeType () {\r\n      this.isdisabled = !this.isdisabled\r\n    },\r\n    async editperson () {\r\n      const { data: res } = await this.axios.post('/modifyPersonInfo', {\r\n        email: this.personForm.email,\r\n        username: this.personForm.username,\r\n        nickname: this.personForm.store_name,\r\n        gender: this.personForm.store_position,\r\n        age: this.personForm.store_phone_num,\r\n        country: this.personForm.store_telephone,\r\n        location: this.personForm.location\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') {\r\n        this.$message.error('修改个人信息失败')\r\n        this.isdisabled = !this.disabled\r\n        this.getpersonInfo()\r\n      }\r\n      this.$message.success('修改个人信息成功')\r\n      this.isdisabled = !this.disabled\r\n      this.getpersonInfo()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.bottombtn {\r\n  float: right;\r\n  margin-bottom: 10px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/logReg"}]}
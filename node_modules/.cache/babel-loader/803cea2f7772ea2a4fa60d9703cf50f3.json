A,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA,CAAA,MAAA;AACA,gBAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KA7CA;AA8CA;AACA,IAAA,gBA/CA,8BA+CA;AACA,WAAA,KAAA,CAAA,WAAA,CAAA,WAAA;AACA,KAjDA;AAmDA;AACA,IAAA,YApDA,0BAoDA;AAAA;;AACA,WAAA,KAAA,CAAA,WAAA,CAAA,QAAA;AAAA,2EAAA,kBAAA,KAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,KADA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAAA;AAAA,yBAGA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,WAAA,MAAA,CAAA,QAAA,CAAA,EAAA,EAAA,MAAA,CAAA,QAAA,CAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA,MAAA,CAHA;;AAAA;AAAA;AAGA,kBAAA,GAHA,yBAGA,IAHA;;AAIA,sBAAA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,oBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA,mBANA,CAQA;;;AACA,kBAAA,MAAA,CAAA,iBAAA,GAAA,KAAA,CATA,CAUA;;AACA,kBAAA,MAAA,CAAA,YAAA,GAXA,CAYA;;;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AAbA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAeA,KApEA;AAqEA;AACA,IAAA,eAtEA,2BAsEA,EAtEA,EAsEA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,MAAA,CAAA,QAAA,CAAA,oBAAA,EAAA,IAAA,EAAA;AACA,kBAAA,iBAAA,EAAA,IADA;AAEA,kBAAA,gBAAA,EAAA,IAFA;AAGA,kBAAA,IAAA,EAAA;AAHA,iBAAA,EAIA,KAJA,CAIA,UAAA,GAAA;AAAA,yBAAA,GAAA;AAAA,iBAJA,CAFA;;AAAA;AAEA,gBAAA,aAFA;;AAAA,sBAOA,aAAA,KAAA,SAPA;AAAA;AAAA;AAAA;;AAAA,kDAQA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CARA;;AAAA;AAAA;AAAA,uBAUA,MAAA,CAAA,OAAA,CAAA,WAAA,EAAA,CAVA;;AAAA;AAAA;AAUA,gBAAA,GAVA,yBAUA,IAVA;;AAAA,sBAWA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAXA;AAAA;AAAA;AAAA;;AAAA,kDAYA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAZA;;AAAA;AAcA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,gBAAA,MAAA,CAAA,WAAA;;AAfA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBA;AAtFA;AArDA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑导航区域 -->\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>订单管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>订单列表</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n\r\n    <!-- 卡片视图区域 -->\r\n    <el-card class=\"box-card\">\r\n      <!-- 搜索与添加区域 -->\r\n       <el-row gutter=\"20\">\r\n         <el-col :span=\"8\">\r\n           <el-input placeholder=\"请输入内容\" v-model=\"queryInfo.query\" clearable\r\n           @clear=\"getOrderList\">\r\n             <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"getOrderList\"></el-button>\r\n          </el-input>\r\n          </el-col>\r\n       </el-row>\r\n\r\n       <!-- 订单列表区 -->\r\n       <el-table :data=\"orderlist\" border stripe>\r\n         <el-table-column type=\"index\"></el-table-column>\r\n         <el-table-column label=\"订单ID\" prop=\"orderid\"></el-table-column>\r\n         <el-table-column label=\"用户名称\" prop=\"orderuname\"></el-table-column>\r\n         <el-table-column label=\"图书名称\" prop=\"orderbname\"></el-table-column>\r\n         <el-table-column label=\"价格\" prop=\"orderprice\"></el-table-column>\r\n         <el-table-column label=\"创建时间\" prop=\"ordercreated\"></el-table-column>\r\n         <el-table-column label=\"订单状态\" prop=\"orderstate\"></el-table-column>\r\n         <el-table-column label=\"操作\" width=\"180px\">\r\n            <template slot-scope=\"scope\">\r\n             <!-- 修改按钮 -->\r\n             <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\" @click=\"showEditDialog(scope.row.id)\"></el-button>\r\n             <!-- 删除按钮 -->\r\n             <el-button type=\"danger\" icon=\"el-icon-delete\" size=\"mini\" @click=\"removeOrderbyId(scope.row.id)\"></el-button>\r\n             <!-- 查看详情 -->\r\n             <el-tooltip class=\"item\" effect=\"dark\" content=\"查看详情\" placement=\"top\" :enterable=\"false\">\r\n               <el-button type=\"warning\" icon=\"el-icon-more\" size=\"mini\" @click=\"showinfoDialog(scope.row.id)\"></el-button>\r\n             </el-tooltip>\r\n           </template>\r\n         </el-table-column>\r\n       </el-table>\r\n\r\n       <!-- 分页区域 -->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryInfo.pagenum\"\r\n        :page-sizes=\"[2, 5, 10]\"\r\n        :page-size=\"queryInfo.pagesize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\">\r\n      </el-pagination>\r\n    </el-card>\r\n\r\n    <!-- 修改对话框 -->\r\n    <el-dialog\r\n      title=\"修改订单\"\r\n      :visible.sync=\"editdialogVisible\"\r\n      width=\"50%\" @close=\"edieDialogClosed\">\r\n      <!-- 内容主体区域-->\r\n      <el-form :model=\"editForm\" :rules=\"editFormRules\" ref=\"editFormRef\" label-width=\"80px\">\r\n        <el-form-item label=\"订单ID\" prop=\"orderid\">\r\n          <el-input autosize v-model=\"editForm.orderid\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"用户名称\" prop=\"orderuname\">\r\n          <el-input autosize v-model=\"editForm.orderuname\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"图书名称\" prop=\"orderbname\">\r\n          <el-input autosize v-model=\"editForm.orderbname\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"价格\" prop=\"orderprice\">\r\n          <el-input autosize v-model=\"editForm.orderprice\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"创建时间\" prop=\"ordercreated\" >\r\n          <el-input autosize v-model=\"editForm.ordercreated\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"订单状态\" prop=\"orderstate\">\r\n          <el-input autosize v-model=\"editForm.orderstate\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"editdialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"editBookInfo\">确 定</el-button>\r\n     </span>\r\n    </el-dialog>\r\n\r\n    <!-- 详情对话框 -->\r\n    <el-dialog\r\n      title=\"订单详情\"\r\n      :visible.sync=\"infodialogVisible\"\r\n      width=\"50%\" @close=\"infodialogClosed\">\r\n      <!-- 内容主体区域-->\r\n      <el-form :model=\"infoForm\" ref=\"infoFormRef\" label-width=\"80px\">\r\n        <el-form-item label=\"订单ID\" prop=\"orderid\">\r\n          <el-input autosize v-model=\"infoForm.orderid\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"用户名称\" prop=\"orderuname\">\r\n          <el-input autosize v-model=\"infoForm.orderuname\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"图书名称\" prop=\"orderbname\">\r\n          <el-input autosize v-model=\"infoForm.orderbname\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"价格\" prop=\"orderprice\">\r\n          <el-input autosize v-model=\"infoForm.orderprice\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"创建时间\" prop=\"ordercreated\">\r\n          <el-input autosize v-model=\"infoForm.ordercreated\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"订单状态\" prop=\"orderstate\">\r\n          <el-input autosize v-model=\"infoForm.orderstate\" disabled></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"infodialogVisible = false\">确 定</el-button>\r\n     </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 获取用订单列表的参数对象\r\n      queryInfo: {\r\n        query: '',\r\n        // 当前页数\r\n        pagenum: '1',\r\n        // 当前每页显示多少条数据\r\n        pagesize: '2'\r\n      },\r\n      orderlist: [],\r\n      total: 0,\r\n      // 控制添加对话框的显示和隐藏\r\n      editdialogVisible: false,\r\n      infodialogVisible: false,\r\n      // 编辑订单的表单数据\r\n      editForm: {\r\n        orderid: '',\r\n        orderuname: '',\r\n        orderbname: '',\r\n        orderprice: '',\r\n        ordercreated: '',\r\n        orderstate: ''\r\n      },\r\n      infoForm: {\r\n        orderid: '',\r\n        orderuname: '',\r\n        orderbname: '',\r\n        orderprice: '',\r\n        ordercreated: '',\r\n        orderstate: ''\r\n      },\r\n      // 添加表单的验证规则对象\r\n      editFormRules: {\r\n        orderuname: [\r\n          { required: true, message: '请输入订单用户名称', trigger: 'blur' }\r\n        ],\r\n        orderbname: [\r\n          { required: true, message: '请输入订单图书名称', trigger: 'blur' }\r\n        ],\r\n        orderprice: [\r\n          { required: true, message: '请输入订单价格', trigger: 'blur' }\r\n        ],\r\n        orderstate: [\r\n          { required: true, message: '请输入订单状态', trigger: 'blur' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.getOrderList()\r\n  },\r\n  methods: {\r\n    async getOrderList () {\r\n      const { data: res } = await this.$http.get('users', { params: this.queryInfo })\r\n      console.log(res)\r\n      if (res.meta.status !== 200) {\r\n        return this.$message.error('获取用户列表失败')\r\n      }\r\n      // this.$message.success('获取用户列表成功')\r\n      this.orderlist = res.data.users\r\n      this.total = res.data.total\r\n      console.log(res)\r\n    },\r\n    // 监听pagesize改变\r\n    handleSizeChange (newSize) {\r\n      console.log(newSize)\r\n      this.queryInfo.pagesize = newSize\r\n      this.getUserList()\r\n    },\r\n    // 监听页码值改变的事件\r\n    handleCurrentChange (newPage) {\r\n      console.log(newPage)\r\n      this.queryInfo.pagenum = newPage\r\n      this.getUserList()\r\n    },\r\n    // 展示编辑订单的对话框\r\n    async showEditDialog (id) {\r\n      this.editdialogVisible = true\r\n      console.log(id)\r\n      const { data: res } = await this.$http.get('users/' + id)\r\n      if (res.mate.status !== 200) {\r\n        return this.$message.error('查询用户信息失败')\r\n      }\r\n      console.log(res.meta.status)\r\n      this.editForm = res.data\r\n    },\r\n    // 展示订单详情的对话框\r\n    async showinfoDialog (id) {\r\n      this.infodialogVisible = true\r\n      console.log(id)\r\n      const { data: res } = await this.$http.get('users/' + id)\r\n      if (res.mate.status !== 200) {\r\n        return this.$message.error('查询用户信息失败')\r\n      }\r\n      console.log(res.meta.status)\r\n      this.editForm = res.data\r\n    },\r\n    // 监听编辑订单的关闭事件\r\n    edieDialogClosed () {\r\n      this.$refs.editFormRef.resetFields()\r\n    },\r\n\r\n    // 修改订单信息并提交\r\n    editBookInfo () {\r\n      this.$refs.editFormRef.validate(async valid => {\r\n        if (!valid) return\r\n        // 发起修改请求\r\n        const { data: res } = await this.$http.put('users/' + this.editForm.id, this.editForm.email, this.editForm.mobile)\r\n        if (res.meta.status !== 200) {\r\n          this.$message.error('修改失败')\r\n        }\r\n\r\n        // 关闭对话框\r\n        this.editdialogVisible = false\r\n        // 刷新数据列表\r\n        this.getOrderList()\r\n        // 提示修改成功\r\n        this.$message.success('修改成功')\r\n      })\r\n    },\r\n    // 根据ID删除\r\n    async removeOrderbyId (id) {\r\n      // 弹框询问用户\r\n      const confirmresult = await this.$confirm('此操作将永久删除该订单, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).catch(err => err)\r\n      if (confirmresult !== 'confirm') {\r\n        return this.$message.info('已取消删除')\r\n      }\r\n      const { data: res } = await this.$delete('users/' + id)\r\n      if (res.mate.status !== 200) {\r\n        return this.$message.error('删除失败')\r\n      }\r\n      this.$message.success('删除成功')\r\n      this.getUserList()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/orders"}]}
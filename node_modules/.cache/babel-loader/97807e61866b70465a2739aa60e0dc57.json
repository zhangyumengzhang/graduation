,QAAA,GAAA,GAAA,CAAA,IAAA;AACA,gBAAA,KAAA,CAAA,KAAA,GAAA,QAAA,CAAA,GAAA,CAAA,GAAA,CAAA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KAfA;AAgBA;AACA,IAAA,gBAjBA,4BAiBA,OAjBA,EAiBA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,WAAA,SAAA,CAAA,QAAA,GAAA,OAAA;AACA,WAAA,WAAA;AACA,KArBA;AAsBA;AACA,IAAA,mBAvBA,+BAuBA,OAvBA,EAuBA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,WAAA,SAAA,CAAA,OAAA,GAAA,OAAA;AACA,WAAA,WAAA;AACA;AA3BA;AAlBA,CAAA","sourcesContent":["/* eslint-disable no-irregular-whitespace */\r\n<template>\r\n  <div>\r\n    <!-- 面包屑导航区域 -->\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>图书借读</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n\r\n    <!-- 卡片视图区域 -->\r\n    <el-card class=\"box-card\">\r\n\r\n       <!-- 图书列表区 -->\r\n       <el-table :data=\"booklist\" border stripe>\r\n         <el-table-column label=\"用户邮箱\" prop=\"user_email\"></el-table-column>\r\n         <el-table-column label=\"图书ID\" prop=\"book_id\"></el-table-column>\r\n         <el-table-column label=\"图书名称\" prop=\"book_name\"></el-table-column>\r\n         <el-table-column label=\"借读时间\" prop=\"lend_time\"></el-table-column>\r\n         <el-table-column label=\"联系方式\" prop=\"telephone\"></el-table-column>\r\n       </el-table>\r\n        <!-- 分页区域 -->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryInfo.pagenum\"\r\n        :page-sizes=\"[1,2, 5, 10]\"\r\n        :page-size=\"queryInfo.pagesize\"\r\n        layout=\"total,sizes,prev, pager, next, jumper\"\r\n        :total=\"total\">\r\n      </el-pagination>\r\n    </el-card>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 获取用户列表的参数对象\r\n      queryInfo: {\r\n        // 当前页数\r\n        pagenum: 1,\r\n        // 当前每页显示多少条数据\r\n        pagesize: 5,\r\n        query: ''\r\n      },\r\n      booklist: [],\r\n      total: 0\r\n    }\r\n  },\r\n  created () {\r\n    this.getBookList()\r\n  },\r\n  methods: {\r\n\r\n    // 获取图书列表\r\n    async getBookList () {\r\n      // 获取的请求\r\n      console.log(this.queryInfo.pagesize)\r\n      const { data: res } = await this.axios.post('/selectlendbook', {\r\n        pagenum: this.queryInfo.pagenum,\r\n        pagesize: this.queryInfo.pagesize\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') return this.$message.error('获取借读图书列表失败')\r\n      this.$message.success('获取借读图书列表成功')\r\n      this.booklist = res.data\r\n      this.total = parseInt(res.sum)\r\n    },\r\n    // 监听pagesize改变\r\n    handleSizeChange (newSize) {\r\n      console.log(newSize)\r\n      this.queryInfo.pagesize = newSize\r\n      this.getBookList()\r\n    },\r\n    // 监听页码值改变的事件\r\n    handleCurrentChange (newPage) {\r\n      console.log(newPage)\r\n      this.queryInfo.pagenum = newPage\r\n      this.getBookList()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.editbtn{\r\n  float: right;\r\n  line-height: 15px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}
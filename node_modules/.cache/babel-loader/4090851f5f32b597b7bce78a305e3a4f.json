AAA,CAAA,aAAA,EAAA;AACA,kBAAA,QAAA,EAAA,MAAA,CAAA,SAAA,CAAA,QADA;AAEA,kBAAA,UAAA,EAAA;AAFA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,GADA,yBACA,IADA;AAKA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AALA,sBAMA,GAAA,CAAA,MAAA,KAAA,GANA;AAAA;AAAA;AAAA;;AAAA,kDAMA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,OAAA,CANA;;AAAA;AAOA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,GAAA,CAAA,OAAA;;AACA,gBAAA,MAAA,CAAA,YAAA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAvEA;AAwEA;AACA,IAAA,UAzEA,wBAyEA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,aAAA,EAAA;AACA,kBAAA,QAAA,EAAA,MAAA,CAAA,SAAA,CAAA,QADA;AAEA,kBAAA,KAAA,EAAA,MAAA,CAAA,SAAA,CAAA;AAFA,iBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,yBAEA,IAFA;AAMA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AANA,sBAOA,GAAA,CAAA,MAAA,KAAA,GAPA;AAAA;AAAA;AAAA;;AAAA,kDAOA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,YAAA,CAPA;;AAAA;AAQA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,YAAA;;AACA,gBAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,SAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA,KAnFA;AAqFA;AACA,IAAA,WAtFA,uBAsFA,SAtFA,EAsFA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,MAAA,CAAA,QAAA,CAAA,oBAAA,EAAA,IAAA,EAAA;AACA,kBAAA,iBAAA,EAAA,IADA;AAEA,kBAAA,gBAAA,EAAA,IAFA;AAGA,kBAAA,IAAA,EAAA;AAHA,iBAAA,EAIA,KAJA,CAIA,UAAA,GAAA;AAAA,yBAAA,GAAA;AAAA,iBAJA,CAFA;;AAAA;AAEA,gBAAA,aAFA;;AAAA,sBAOA,aAAA,KAAA,SAPA;AAAA;AAAA;AAAA;;AAAA,kDAQA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CARA;;AAAA;AAWA,gBAAA,OAAA,CAAA,GAAA,CAAA,SAAA;AAXA;AAAA,uBAYA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,EAAA;AACA,kBAAA,QAAA,EAAA,MAAA,CAAA,cAAA,CAAA,OAAA,CAAA,UAAA,CADA;AAEA,kBAAA,SAAA,EAAA;AAFA,iBAAA,CAZA;;AAAA;AAAA;AAYA,gBAAA,GAZA,yBAYA,IAZA;AAgBA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AAhBA,sBAiBA,GAAA,CAAA,MAAA,KAAA,GAjBA;AAAA;AAAA;AAAA;;AAAA,kDAiBA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,OAAA,CAjBA;;AAAA;AAkBA,oBAAA,GAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,GAAA,CAAA,OAAA;;AACA,kBAAA,MAAA,CAAA,YAAA;AACA;;AArBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsBA;AA5GA,GA1BA;AAwIA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,cAFA,0BAEA,KAFA,EAEA;AACA,aAAA,KAAA,GAAA,IAAA,GAAA,IAAA;AACA,KAJA;;AAKA;AACA,IAAA,aANA,2BAMA;AAAA,UAAA,MAAA,uEAAA,CAAA;AACA,aAAA,gBAAA,CAAA,MAAA,CAAA;AACA;AARA;AAxIA,CAAA","sourcesContent":["<template>\r\n  <div >\r\n    <!-- 面包屑导航区域 -->\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/welcome' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>收藏夹</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <!-- 卡片视图区域 -->\r\n\r\n    <el-card class=\"box-card\">\r\n       <!-- 搜索与添加区域 -->\r\n       <el-row gutter=\"20\">\r\n         <el-col :span=\"8\">\r\n           <el-input placeholder=\"请输入内容\" v-model=\"queryInfo.query\" clearable\r\n           @clear=\"getmusicList\">\r\n             <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"queryaudio\"></el-button>\r\n          </el-input>\r\n          </el-col>\r\n       </el-row>\r\n         <!-- 播放进度条 -->\r\n       <div id='playMusic'>\r\n          <audio ref='audio' :src='src'\r\n            @pause=\"onPause\"\r\n            @play='onplay'\r\n            @timeupdate=\"onTimeupdate\"\r\n            @loadedmetadata=\"onLoadedmetadata\"></audio>\r\n          <!-- 进度条 -->\r\n          <el-slider v-model=\"sliderTime\" :format-tooltip=\"formatProcessToolTip\" @change=\"changeCurrentTime\" class=\"slider\"></el-slider>\r\n          <span>{{audio.currentTime | formartSecond}} / </span>\r\n          <span>{{audio.maxTime | formartSecond}}</span>\r\n        </div>\r\n       <!-- 类别列表区 -->\r\n       <el-table :data=\"musiclist\" border stripe>\r\n         <el-table-column label=\"音乐名称\" prop=\"audiotitle\"></el-table-column>\r\n         <el-table-column label=\"音乐类型\" prop=\"type\"></el-table-column>play\r\n          <el-table-column label=\"播放\" width=\"140px\">\r\n            <template slot-scope=\"scope\">\r\n             <!-- 播放 -->\r\n            <el-button type='primary' size=\"mini\" icon=\"el-icon-video-play\" @click='startPlayOrPause(scope.row.audiotitle)'></el-button>\r\n             <!-- <el-button type=\"primary\" icon=\"el-icon-video-play\" size=\"mini\" @click=\"handlePlay('audio')\"></el-button> -->\r\n           </template>\r\n         </el-table-column>\r\n         <el-table-column label=\"取消收藏\" width=\"140px\">\r\n            <template slot-scope=\"scope\">\r\n             <!-- 收藏 -->\r\n             <el-button type=\"primary\" icon=\"el-icon-star-on\" size=\"mini\" @click=\"changestar(scope.row.audiotitle)\"></el-button>\r\n           </template>\r\n         </el-table-column>\r\n         <el-table-column label=\"下载\" width=\"140px\">\r\n            <template slot-scope=\"scope\">\r\n             <!-- 下载 -->\r\n             <el-button type=\"primary\" icon=\"el-icon-download\" size=\"mini\" @click=\"download(scope.row.audiotitle)\"></el-button>\r\n           </template>\r\n         </el-table-column>\r\n         <el-table-column label=\"删除\" width=\"140px\">\r\n            <template slot-scope=\"scope\">\r\n             <!-- 删除 -->\r\n             <el-button type=\"primary\" icon=\"el-icon-delete\" size=\"mini\" @click=\"deleteaudio(scope.row.audiotitle)\"></el-button>\r\n           </template>\r\n         </el-table-column>\r\n       </el-table>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// 将整数转换成 时：分：秒的格式\r\nfunction realFormatSecond (second) {\r\n  var secondType = typeof second\r\n\r\n  if (secondType === 'number' || secondType === 'string') {\r\n    second = parseInt(second)\r\n\r\n    var hours = Math.floor(second / 3600)\r\n    second = second - hours * 3600\r\n    var mimute = Math.floor(second / 60)\r\n    second = second - mimute * 60\r\n\r\n    return hours + ':' + ('0' + mimute).slice(-2) + ':' + ('0' + second).slice(-2)\r\n  } else {\r\n    return '0:00:00'\r\n  }\r\n}\r\nexport default {\r\n  data () {\r\n    return {\r\n      sliderTime: 0,\r\n      audio: {\r\n        maxTime: 0, /* 音频最大播放时长 */\r\n        currentTime: 0, /* 当前播放时长 */\r\n        playing: false /* 音频当前处于播放/暂停状态 */\r\n      },\r\n      musiclist: [],\r\n      queryInfo: {\r\n        username: window.sessionStorage.getItem('username'),\r\n        query: ''\r\n      }\r\n    }\r\n  },\r\n  name: 'playMusic',\r\n  props: {\r\n    src: {\r\n      type: String,\r\n      default: ''\r\n    }\r\n  },\r\n  created () {\r\n    this.getmusicList()\r\n  },\r\n  methods: {\r\n\r\n    /* 进度条格式化toolTip */\r\n    formatProcessToolTip (index = 0) {\r\n      index = parseInt(this.audio.maxTime / 100 * index)\r\n      return '时长：' + realFormatSecond(index)\r\n    },\r\n    /* 拖动进度条，改变当前时间 index是进度条改变时的回调函数的参数 值为0~100之间，需要换算成实际时间 */\r\n    changeCurrentTime (index) {\r\n      this.$refs.audio.currentTime = parseInt(index / 100 * this.audio.maxTime)\r\n    },\r\n    /* 音频加载完成后的回调函数 */\r\n    onLoadedmetadata (res) {\r\n      console.log(111)\r\n      this.audio.maxTime = parseInt(res.target.duration)\r\n    },\r\n    /* 每秒触发一次 用来更新当前播放时间 */\r\n    onTimeupdate (res) {\r\n      this.audio.currentTime = res.target.currentTime\r\n      /* 当音频播放时 进度条也要随之改变 */\r\n      this.sliderTime = parseInt(this.audio.currentTime / this.audio.maxTime * 100)\r\n    },\r\n    /* 控制音频播放、暂停 */\r\n    startPlayOrPause (audiotitle) {\r\n      if (this.audio.playing) {\r\n        this.pause()\r\n      } else {\r\n        this.src = 'http://localhost:8900/static/' + audiotitle\r\n        this.play()\r\n      }\r\n    },\r\n    /* 播放音频 */\r\n    play () {\r\n      this.$refs.audio.play()\r\n    },\r\n    /* 暂停音频 */\r\n    pause () {\r\n      this.$refs.audio.pause()\r\n    },\r\n    /* 当音频播放 */\r\n    onplay () {\r\n      this.audio.playing = true\r\n    },\r\n    /* 当音频暂停 */\r\n    onPause () {\r\n      this.audio.playing = false\r\n    },\r\n\r\n    // 获取音频\r\n    async getmusicList () {\r\n      const { data: res } = await this.axios.post('/getstar', {\r\n        username: window.sessionStorage.getItem('username')\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') return this.$message.error(res.message)\r\n\r\n      if (res.status === '1') {\r\n        this.$message.success(res.message)\r\n        this.musiclist = res.songlists\r\n      };\r\n    },\r\n    // 改变收藏状态\r\n    async changestar (audiotitle) {\r\n      const { data: res } = await this.axios.post('/canclestar', {\r\n        username: this.queryInfo.username,\r\n        audiotitle: audiotitle\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') return this.$message.error(res.message)\r\n      this.$message.success(res.message)\r\n      this.getmusicList()\r\n    },\r\n    // 模糊查询音频\r\n    async queryaudio () {\r\n      // 获取的请求\r\n      const { data: res } = await this.axios.post('/queryaudio', {\r\n        username: this.queryInfo.username,\r\n        query: this.queryInfo.query\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') return this.$message.error('模糊获取音频列表失败')\r\n      this.$message.success('模糊获取音频列表成功')\r\n      this.musiclist = res.songlists\r\n    },\r\n\r\n    // 删除音频\r\n    async deleteaudio (musicname) {\r\n      // 弹框询问用户\r\n      const confirmresult = await this.$confirm('此操作将永久删除该音频, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).catch(err => err)\r\n      if (confirmresult !== 'confirm') {\r\n        return this.$message.info('已取消删除')\r\n      }\r\n\r\n      console.log(musicname)\r\n      const { data: res } = await this.axios.post('/delete', {\r\n        username: window.sessionStorage.getItem('username'),\r\n        musicname: musicname\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') return this.$message.error(res.message)\r\n      if (res.status === '1') {\r\n        this.$message.success(res.message)\r\n        this.getmusicList()\r\n      }\r\n    }\r\n  },\r\n  filters: {\r\n    /* 使用vue过滤器动态改变按钮的显示 */\r\n    transPlayPause (value) {\r\n      return value ? '暂停' : '播放'\r\n    },\r\n    /* 整数转换时分秒 */\r\n    formartSecond (second = 0) {\r\n      return realFormatSecond(second)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n#playMusic {\r\n  display: flex;\r\n  align-items: center;\r\n  .slider {\r\n    width: 150px;\r\n    margin-left: 570px;\r\n  }\r\n  > span:nth-of-type(2) {\r\n    margin-left: 5px;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}
AA,OAAA,EAAA;AAAA,kBAAA,MAAA,EAAA,KAAA,CAAA;AAAA,iBAAA,CAFA;;AAAA;AAAA;AAEA,gBAAA,GAFA,yBAEA,IAFA;AAGA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AAHA,sBAIA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAJA;AAAA;AAAA;AAAA;;AAAA,iDAKA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,UAAA,CALA;;AAAA;AAOA;AACA,gBAAA,KAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA,KAXA;AAYA,IAAA,UAZA,wBAYA;AACA,WAAA,UAAA,GAAA,CAAA,KAAA,UAAA;AACA;AAdA;AApCA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑导航区域 -->\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>店铺管理</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <!-- 卡片视图区域 -->\r\n    <el-card class=\"box-card\">\r\n      <div slot=\"header\" class=\"clearfix\">\r\n        <span>店铺信息</span>\r\n        <el-button style=\"float: right; padding: 3px 0\" type=\"text\" @click=\"changeType\">修改</el-button>\r\n      </div>\r\n    <!-- 内容主体区域-->\r\n      <el-form :model=\"shopForm\" :rules=\"shopFormRules\" ref=\"shopFormRef\" label-width=\"80px\">\r\n        <el-form-item label=\"店铺名称\" prop=\"shopname\">\r\n          <el-input v-model=\"shopForm.shopname\" :disabled=\"isdisabled\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"店铺地址\" prop=\"shopaddress\">\r\n          <el-input autosize v-model=\"shopForm.shopaddress\" :disabled=\"isdisabled\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"移动电话\" prop=\"mobile\">\r\n          <el-input v-model=\"shopForm.mobile\" :disabled=\"isdisabled\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"固定电话\" prop=\"telephone\">\r\n          <el-input v-model=\"shopForm.telephone\" :disabled=\"isdisabled\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"创建时间\" prop=\"created\">\r\n          <el-input v-model=\"shopForm.created\" :disabled=\"isdisabled\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span class=\"bottombtn\" :hidden=\"isdisabled\">\r\n        <el-button  type=\"info\"  @click=\"changeType()\">取 消</el-button>\r\n        <el-button  type=\"primary\" @click=\"changeType()\">保 存</el-button>\r\n      </span>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 获取用户列表的参数对象\r\n      isdisabled: true,\r\n      // 添加用户的表单数据\r\n      shopForm: {\r\n        shopname: '',\r\n        shopaddress: '',\r\n        mobile: '',\r\n        telephone: '',\r\n        created: ''\r\n      },\r\n      // 添加表单的验证规则对象\r\n      shopFormRules: {\r\n        shopname: [\r\n          { required: true, message: '请输入店铺名', trigger: 'blur' }\r\n        ],\r\n        shopaddress: [\r\n          { required: true, message: '请输入店铺地址', trigger: 'blur' }\r\n        ],\r\n        mobile: [\r\n          { required: true, message: '请输入店铺移动电话', trigger: 'blur' }\r\n        ],\r\n        telephone: [\r\n          { required: true, message: '请输入店铺联系方式', trigger: 'blur' }\r\n        ],\r\n        created: [\r\n          { required: true, message: '请输入店铺成立时间', trigger: 'blur' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created () {\r\n    this.getshopInfo()\r\n  },\r\n  methods: {\r\n    async getshopInfo () {\r\n      // 获取店铺信息请求\r\n      const { data: res } = await this.$http.get('users', { params: this.queryInfo })\r\n      console.log(res)\r\n      if (res.meta.status !== 200) {\r\n        return this.$message.error('获取用户列表失败')\r\n      }\r\n      // this.$message.success('获取用户列表成功')\r\n      this.shopForm = res.data\r\n      console.log(res)\r\n    },\r\n    changeType () {\r\n      this.isdisabled = !this.isdisabled\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.bottombtn {\r\n  float: right;\r\n  margin-bottom: 10px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/shop"}]}
AAA,QAAA;AAFA;AAAA,uBAGA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,kBAAA,EAAA;AACA,kBAAA,QAAA,EAAA,MAAA,CAAA,SAAA,CAAA,QADA;AAEA;AACA,kBAAA,OAAA,EAAA,MAAA,CAAA,SAAA,CAAA,OAHA;AAIA,kBAAA,QAAA,EAAA,MAAA,CAAA,SAAA,CAAA,QAJA;AAKA,kBAAA,KAAA,EAAA,MAAA,CAAA,SAAA,CAAA;AALA,iBAAA,CAHA;;AAAA;AAAA;AAGA,gBAAA,GAHA,yBAGA,IAHA;AAUA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AAVA,sBAWA,GAAA,CAAA,MAAA,KAAA,GAXA;AAAA;AAAA;AAAA;;AAAA,kDAWA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,YAAA,CAXA;;AAAA;AAYA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,YAAA;;AACA,gBAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA;AACA,gBAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,GAAA,CAAA,GAAA,CAAA;;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,CAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,MAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,QAAA;;AACA,sBAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,QAAA,KAAA,CAAA,EAAA;AACA,oBAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,QAAA,GAAA,IAAA;AACA,mBAFA,MAEA;AACA,oBAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,QAAA,GAAA,IAAA;AACA;AACA,iBARA;;AAfA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAwBA,KAnEA;AAoEA;AACA,IAAA,mBArEA,+BAqEA,OArEA,EAqEA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,WAAA,SAAA,CAAA,OAAA,GAAA,OAAA;AACA,WAAA,WAAA;AACA,KAzEA;AA0EA,IAAA,gBA1EA,8BA0EA;AACA,WAAA,KAAA,CAAA,WAAA,CAAA,WAAA;AACA;AA5EA;AAvDA,CAAA","sourcesContent":["/* eslint-disable no-irregular-whitespace */\r\n<template>\r\n  <div>\r\n    <!-- 面包屑导航区域 -->\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>图书借读</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n\r\n    <!-- 卡片视图区域 -->\r\n    <el-card class=\"box-card\">\r\n\r\n       <!-- 图书列表区 -->\r\n       <el-table :data=\"booklist\" border stripe>\r\n         <el-table-column label=\"图书ID\" prop=\"book_id\"></el-table-column>\r\n         <el-table-column label=\"图书名称\" prop=\"title\"></el-table-column>\r\n         <el-table-column label=\"出版社\" prop=\"press\"></el-table-column>\r\n         <el-table-column label=\"作者\" prop=\"author\"></el-table-column>\r\n         <el-table-column label=\"简介\" prop=\"outline\"></el-table-column>\r\n         <el-table-column label=\"出版日期\" prop=\"publish_date\"></el-table-column>\r\n         <el-table-column label=\"操作\" width=\"180px\">\r\n            <template slot-scope=\"scope\">\r\n              <el-tooltip class=\"item\" effect=\"dark\" content=\"添加借读信息\" placement=\"top\" :enterable=\"false\">\r\n              <el-button type=\"danger\" icon=\"el-icon-notebook-2\" size=\"mini\" @click=\"lendbook(scope.row)\"></el-button>\r\n              </el-tooltip>\r\n           </template>\r\n         </el-table-column>\r\n       </el-table>\r\n        <!-- 分页区域 -->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryInfo.pagenum\"\r\n        :page-sizes=\"[1,2, 5, 10]\"\r\n        :page-size=\"queryInfo.pagesize\"\r\n        layout=\"total,prev, pager, next, jumper\"\r\n        :total=\"total\">\r\n      </el-pagination>\r\n    </el-card>\r\n\r\n    <!-- 借读对话框 -->\r\n    <el-dialog\r\n      title=\"发布借读图书\"\r\n      :visible.sync=\"lenddialogVisible\"\r\n      width=\"50%\" @close=\"lenddialogClosed\">\r\n      <!-- 内容主体区域-->\r\n      <el-form :model=\"lendForm\" ref=\"lendFormRef\" label-width=\"100px\" :rules=\"lendFormRules\" >\r\n        <el-form-item label=\"图书ID\" prop=\"book_id\" style=\"margin-top:-20px\">\r\n          <el-input type=\"textarea\" autosize v-model=\"lendForm.book_id\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"图书名称\" prop=\"name\">\r\n          <el-input type=\"textarea\" autosize v-model=\"lendForm.name\" :disabled=\"isdisabled\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"联系方式\" prop=\"mobile\">\r\n          <el-input type=\"textarea\" autosize v-model=\"lendForm.mobile\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <el-form style=\"text-align:right\">\r\n        <el-select v-model=\"lendForm.days\" placeholder=\"选择借读天数\" style=\"margin-right: 3%\">\r\n            <el-option\r\n              v-for=\"item in options\"\r\n              :key=\"item.value\"\r\n              :label=\"item.label\"\r\n              :value=\"item.value\">\r\n            </el-option>\r\n         </el-select>\r\n          <el-button style=\"margin-top:-70px\" type=\"primary\" @click=\"confirmlend()\">确 定</el-button>\r\n      </el-form>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      // 获取用户列表的参数对象\r\n      queryInfo: {\r\n        // 当前页数\r\n        pagenum: 1,\r\n        query: ''\r\n      },\r\n      options: [{\r\n        value: '1',\r\n        label: '1天'\r\n      }, {\r\n        value: '2',\r\n        label: '2天'\r\n      }, {\r\n        value: '3',\r\n        label: '3天'\r\n      }, {\r\n        value: '4',\r\n        label: '4天'\r\n      }, {\r\n        value: '5',\r\n        label: '5天'\r\n      }, {\r\n        value: '6',\r\n        label: '6天'\r\n      }, {\r\n        value: '7',\r\n        label: '7天'\r\n      }],\r\n      booklist: [],\r\n      total: 0,\r\n      lenddialogVisible: false,\r\n      isdisabled: true,\r\n\r\n      lendForm: {\r\n        book_id: '',\r\n        name: '',\r\n        mobile: '',\r\n        days: ''\r\n      },\r\n      lendFormRules: {\r\n        days: [\r\n          { required: true, message: '请选择借读天数', trigger: 'blur' }\r\n        ],\r\n        phone: [\r\n          { required: true, message: '请填写联系方式', trigger: 'blur' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created () {\r\n    this.getBookList()\r\n  },\r\n  methods: {\r\n\r\n    // 展示借读对话框\r\n    lendbook (book) {\r\n      this.lendForm.book_id = book.book_id\r\n      this.lendForm.name = book.title\r\n      this.lenddialogVisible = true\r\n    },\r\n    // 确定借读\r\n    async confirmlend () {\r\n      const { data: res } = await this.axios.post('/lendbook', {\r\n        user_email: '17301115@bjtu.edu.cn',\r\n        book_id: this.lendForm.book_id,\r\n        book_name: this.lendForm.name,\r\n        lend_time: this.lendForm.days,\r\n        telephone: this.lendForm.mobile\r\n      }\r\n      )\r\n      console.log(res)\r\n      if (res.status !== '1') return this.$message.error('添加借读信息失败')\r\n      this.$message.success('添加借读信息成功')\r\n    },\r\n    // 获取图书列表\r\n    async getBookList () {\r\n      // 获取的请求\r\n      console.log(this.queryInfo.pagesize)\r\n      const { data: res } = await this.axios.post('/selectBookAll', {\r\n        page: this.queryInfo.pagenum\r\n      })\r\n      console.log(res)\r\n      if (res.status_code !== '1') return this.$message.error('获取图书列表失败')\r\n      this.$message.success('获取图书列表成功')\r\n      this.booklist = res.data\r\n      this.total = parseInt(res.sum)\r\n      this.booklist.forEach((item, i) => {\r\n        if (res.data[i].is_shelf === 1) {\r\n          res.data[i].is_shelf = '上架'\r\n        } else {\r\n          res.data[i].is_shelf = '下架'\r\n        }\r\n      })\r\n    },\r\n    // 模糊查询图书\r\n    async selectBookQuery () {\r\n      // 获取的请求\r\n      console.log(this.queryInfo.pagesize)\r\n      const { data: res } = await this.axios.post('/selectBookQuery', {\r\n        store_id: this.queryInfo.store_id,\r\n        // store_id: 1,\r\n        pagenum: this.queryInfo.pagenum,\r\n        pagesize: this.queryInfo.pagesize,\r\n        query: this.queryInfo.query\r\n      })\r\n      console.log(res)\r\n      if (res.status !== '1') return this.$message.error('模糊获取图书列表失败')\r\n      this.$message.success('模糊获取图书列表成功')\r\n      this.booklist = res.data\r\n      this.total = parseInt(res.sum)\r\n      this.booklist.forEach((item, i) => {\r\n        console.log(res.data[i].author)\r\n        console.log(res.data[i].is_shelf)\r\n        if (res.data[i].is_shelf === 1) {\r\n          res.data[i].is_shelf = '上架'\r\n        } else {\r\n          res.data[i].is_shelf = '下架'\r\n        }\r\n      })\r\n    },\r\n    // 监听页码值改变的事件\r\n    handleCurrentChange (newPage) {\r\n      console.log(newPage)\r\n      this.queryInfo.pagenum = newPage\r\n      this.getBookList()\r\n    },\r\n    lenddialogClosed () {\r\n      this.$refs.lendFormRef.resetFields()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.editbtn{\r\n  float: right;\r\n  line-height: 15px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}